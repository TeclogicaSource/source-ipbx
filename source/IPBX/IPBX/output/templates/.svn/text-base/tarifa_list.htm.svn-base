<html {$html_attrs}>
<head>
<title>Tarifa</title>
<link REL="stylesheet" href="include/style.css" type="text/css">
<!--[if IE]>
<link REL="stylesheet" href="include/styleIE.css" type="text/css">
<![endif]-->
{BEGIN style_block}
<style>
#height100{$id} {height:auto;padding:10px;}
/*#search_block{$id} div {padding:5px 7px;}*/
#global_block{$id} {padding:5px 12px;}
#search_block_center{$id} {padding:3px 5px;text-align:center;}
#center_block{$id} {text-align:center;padding:4px;}
#center_block_caption{$id} {text-align:center;padding:4px;}
#left_block{$id} { position:absolute;width:202px; {BEGIN RTL_block} right:20px;{END RTL_block}{BEGIN LTR_block} left:20px;{END LTR_block} overflow:visible; margin-top:10px}
#right_block{$id} { padding:10px 0px 0px 0px;}
{BEGIN shiftstyle_block}
#right_block{$id} {padding-left:220px;{BEGIN RTL_block}padding-left:0px;padding-right:220px;{END RTL_block}}
{END shiftstyle_block}
#toplinks_block{$id} {padding:0 5px 5px 5px;}
#toplinks_block{$id} > * {margin:4px 6px;vertical-align:middle;}
#menu_block{$id} {clear:left;padding:5px 0px; width:auto;}
#details_block{$id}, #pages_block{$id}, #records_pp_block{$id} {white-space:nowrap;padding:5px 7px;text-align:center;}
#recordspp_block{$id} {padding:10px;}
#recordcontrols_block{$id} {padding:13px 10px 9px 10px;text-align:left}
#record_controls{$id} > *,#newrecord_controls{$id} > * {margin:2px 2px;vertical-align:middle}
.listIcons img.listIcon{margin:0 3px;opacity:0.3;filter:alpha(opacity=30);}
.menuitem {padding:6px 18px 10px 13px}
.menuitem_active {padding:6px 18px 11px 13px}
.message_block {text-align:center;}
.grid_recordheader {padding:0 5px 5px}
.grid_recordheader a {margin:0px 5px;}
#grid_block{$id} {margin-top:10px;}
.headerlist_right2 *,.headerlist * {vertical-align:middle}
body{margin:0px;padding:0px;}

#searchPanelContainer{$id}{
	width: 200px

}

/*report styles*/
#export_links{$id} * {padding:0}
</style>
{END style_block}
<!--[if IE]>
{BEGIN iestyle_block}
<style>
#right_block{$id} { width:100%}
/*#global_block{$id} {height:95%}*/
</style>
{END iestyle_block}
<![endif]-->
</head>

<body marginheight=0 marginwidth=0 {$bodyattrs}>
{BEGIN body}
<div id="height100{$id}" layout="London">
{$header}
<b class="xtop"><b class="xb1b"></b><b class="xb2b"></b><b class="xb3b"></b><b class="xb4b"></b></b>
{BEGIN toplinks_block}
<div id="toplinks_block{$id}" class="upeditmenu">
<span><font style="FONT-FAMILY: Verdana, Arial;">Logado Como</font>&nbsp;<b>{$username}</b>&nbsp;</span>
<span class=buttonborder><input type=button class=button value="Log Out" {$logoutlink_attrs}></span>

{BEGIN adminarea_link}
<span class=buttonborder><input type=button class=button value="Área  Admin" {$adminarealink_attrs}></span>
{END adminarea_link}

{BEGIN changepwd_link}
<span class=buttonborder><input type=button class=button value="Mudar senha" {$changepwdlink_attrs}></span>
{END changepwd_link}

{BEGIN asearch_link}
<span class=buttonborder><input type=button class=button value="Busca Avancada" {$asearchlink_attrs}></span>
{END asearch_link}



{BEGIN print_link}
<span class=buttonborder><input type=button class=button value="Imprimir esta página" {$printlink_attrs}></span>
{END print_link}

{BEGIN printall_link}
<span class=buttonborder><input type=button class=button value="Imprimir todas as Páginas" {$printalllink_attrs}></span>
{END printall_link}



</div>
{END toplinks_block}
<div class="main_table_border2 main_table" id="global_block{$id}">
{BEGIN left_block}
<div id="left_block{$id}" >
    <b class="xtop"><b class="xb1b1"></b><b class="xb2b1"></b><b class="xb3b1"></b><b class="xb4b1"></b></b>
	<!-- MenuBegin -->
{BEGIN menu_block}
<div class="menu_table" id="menu_block{$id}">
<!-- MainMenuBegin -->
{$mainmenu}
<!-- MainMenuEnd -->
</div>
{END menu_block}
		
	<!-- MenuEnd -->	
	{BEGIN search_records_block}
	<div>
        		{BEGIN searchform}
        <div class="menu_table" id="search_block{$id}">
        
           <!-- search panel and window -->
           <style>
<!--
#center_block{$id}{
	padding: 5px;
}
#searchPanelHeader{$id}{
	padding: 5px 0px 5px 5px;
}
-->
</style>

<!-- search panel and window header -->
<div class="blackshade2" id="center_block{$id}">
	Buscar:
	&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
	<span class="buttonborder"><input class="button" type="button" id="showSrchWin{$id}" style="width: 18px; background-image: url(images/search/showWindowPin.gif);" {$showHideSearchWin_attrs} ></span>
	&nbsp;		
	<span class="buttonborder"><input class="button" type="button" id="showOptPanel{$id}" style="width: 18px; background-image: url(images/search/showOptions.gif);" {$showHideSearchPanel_attrs} ></span>

</div>
<div id="searchform{$id}">
	<div class="blackshade" id="searchPanelHeader{$id}">
		{$simpleSearchFieldCombo}
		{$simpleSearchTypeCombo}
		
		{BEGIN searchform_text}
			<input type=text {$searchfor_attrs} size="25">
		{END searchform_text}
		&nbsp;
		{BEGIN searchform_search}
			<span class="buttonborder"><input class="button" type="button" style="width: 18px; background-image: url(images/search/search.gif);" {$searchbutton_attrs} /></span>
		{END searchform_search}
		&nbsp;		
		{BEGIN searchform_showall}
			<span id="showall{$id}" class=buttonborder><input type=button class="button" value="Mostrar Todos" {$showallbutton_attrs}></span>
		{END searchform_showall}
	</div>
</div>  
			<div id="searchPanelContainer{$id}" class="searchPanelContainer">			
			{$adv_search_panel}
			</div>                      
            
            <div class="blackshade2" id="details_block{$id}">
                {BEGIN details_block}
                <span id="detFound{$id}">Ítens Encontrados: {$records_found}</span>
                {END details_block}
            </div>
            <div class="menu_text blackshade" id="pages_block{$id}">
                {BEGIN pages_block}
                <span id="pageOf{$id}">Página {$page} de {$maxpages}</span>
                {END pages_block}
            </div>
        </div>
		{END searchform}
        <div class="menu_table2">
        
        	            {BEGIN recordspp_block}
            <div class="menu_text blackshade" id="recordspp_block{$id}" >
                Registros por Página:&nbsp;
                <select {$recordspp_attrs}>
	                <option value="10" {$rpp10_selected}>10</option>
	                <option value="20" {$rpp20_selected}>20</option>
	                <option value="30" {$rpp30_selected}>30</option>
	                <option value="50" {$rpp50_selected}>50</option>
	                <option value="100" {$rpp100_selected}>100</option>
	                <option value="500" {$rpp500_selected}>500</option>
                </select>
            </div>
            {END recordspp_block}
        </div>
	</div>
	{END search_records_block}
    </div>
{END left_block}
    <div id="right_block{$id}">
        
        
        <div class=list_table1>
        <div class=list_table>
        
        <div class=blackshade2 id="center_block_caption{$id}">
        Tarifa
        </div>
        {BEGIN recordcontrols_block}
        <div class=blackshade id="recordcontrols_block{$id}">
	        {BEGIN newrecord_controls}
		<span id="newrecord_controls{$id}">
        
                {BEGIN inlineadd_link}
        <span class=buttonborder><input type=button class="button" value="Adicionar Novo" {$inlineaddlink_attrs} ></span>
        {END inlineadd_link}
        </span>
		{END newrecord_controls}
		
		{$lookupSearchControls}
		
		{BEGIN record_controls}
		<span id="record_controls{$id}">
                        {BEGIN selectall_link}
        <span class=buttonborder {$selectalllink_span}><input type=button class="button" value="Marcar/Desmarcar todos" {$selectalllink_attrs}></span>
        {END selectall_link}

                        {BEGIN editselected_link}
        <span class=buttonborder {$editselectedlink_span}> <input type=button class="button" value="Editar selecionado" {$editselectedlink_attrs}></span>
        {END editselected_link}
        {BEGIN saveall_link}
        <span class=buttonborder {$savealllink_span}><input type=button class="button" value="Salvar todos" {$savealllink_attrs}></span>
        {END saveall_link}
        {BEGIN cancelall_link}
        <span class=buttonborder {$cancelalllink_span}><input type=button class="button" value="Cancelar Edição" {$cancelalllink_attrs}></span>
        {END cancelall_link}

                {BEGIN deleteselected_link}
        <span class=buttonborder {$deleteselectedlink_span}><input type=button class="button" value="Elimina Selecionados" {$deleteselectedlink_attrs}></span>
        {END deleteselected_link}

        
                {BEGIN printselected_link}
        <span class=buttonborder {$printselectedlink_span}><input type=button class="button" value="Impressão selecionada" {$printselectedlink_attrs}></span>
        {END printselected_link}
		</span>
		{END record_controls}
        </div>
        {END recordcontrols_block}
		<div class="message" id="usermessage{$id}">{$user_message}</div>
{BEGIN grid_block}
<div id="grid_block{$id}">
<table border="0" cellpadding=3 cellspacing=0 name="maintable">
{BEGIN grid_row}
<tr  {$rowattrs} style="vertical-align:top">
{BEGIN grid_record}
<td class=headerlistdown_v style="vertical-align:top">
{BEGIN grid_vrecord}
<table cellspacing="0" cellpadding="0" border="0">
{BEGIN grid_recordheader}
<tr>
<td colspan=2 class="grid_recordheader">

{BEGIN inlineedit_column}
{BEGIN inlineedit_link}
<a class="tablelinks2" {$inlineeditlink_attrs} title="Editar"><img src="include/img/icon_inline_edit.gif" alt="Editar" border="0"></a>
{END inlineedit_link}
{END inlineedit_column}



{BEGIN checkbox_column}
{BEGIN checkbox}
<input type=checkbox {$checkbox_attrs}>
{END checkbox}
{END checkbox_column}

</td>
</tr>
{END grid_recordheader}
{BEGIN dsc_tarifa_fieldcolumn}
<tr><td class=headerlist_v width=30% style="padding-left:10px">Descrição</td>
<td class=editshade_v style="padding-left:10px" align="left">
{$dsc_tarifa_value}&nbsp;
{END dsc_tarifa_fieldcolumn}
</td></tr>
{BEGIN hr_inicio_fieldcolumn}
<tr><td class=headerlist_v width=30% style="padding-left:10px">Hora Inicio</td>
<td class=editshade_v style="padding-left:10px" align="left">
{$hr_inicio_value}&nbsp;
{END hr_inicio_fieldcolumn}
</td></tr>
{BEGIN hr_fim_fieldcolumn}
<tr><td class=headerlist_v width=30% style="padding-left:10px">Hora Fim</td>
<td class=editshade_v style="padding-left:10px" align="left">
{$hr_fim_value}&nbsp;
{END hr_fim_fieldcolumn}
</td></tr>
{BEGIN vlr_unid_fieldcolumn}
<tr><td class=headerlist_v width=30% style="padding-left:10px">Valor Unitário</td>
<td class=editshade_v style="padding-left:10px" align="left">
{$vlr_unid_value}&nbsp;
{END vlr_unid_fieldcolumn}
</td></tr>
{BEGIN cad_inicial_fieldcolumn}
<tr><td class=headerlist_v width=30% style="padding-left:10px">Cadencia Inicial</td>
<td class=editshade_v style="padding-left:10px" align="left">
{$cad_inicial_value}&nbsp;
{END cad_inicial_fieldcolumn}
</td></tr>
{BEGIN cad_frac_fieldcolumn}
<tr><td class=headerlist_v width=30% style="padding-left:10px">Cadencia Fracionaria</td>
<td class=editshade_v style="padding-left:10px" align="left">
{$cad_frac_value}&nbsp;
{END cad_frac_fieldcolumn}
</td></tr>
{BEGIN seg_fieldcolumn}
<tr><td class=headerlist_v width=30% style="padding-left:10px">Segunda Feira</td>
<td class=editshade_v style="padding-left:10px" align="left">
{$seg_value}&nbsp;
{END seg_fieldcolumn}
</td></tr>
{BEGIN ter_fieldcolumn}
<tr><td class=headerlist_v width=30% style="padding-left:10px">Terça Feira</td>
<td class=editshade_v style="padding-left:10px" align="left">
{$ter_value}&nbsp;
{END ter_fieldcolumn}
</td></tr>
{BEGIN qua_fieldcolumn}
<tr><td class=headerlist_v width=30% style="padding-left:10px">Quarta Feira</td>
<td class=editshade_v style="padding-left:10px" align="left">
{$qua_value}&nbsp;
{END qua_fieldcolumn}
</td></tr>
{BEGIN qui_fieldcolumn}
<tr><td class=headerlist_v width=30% style="padding-left:10px">Quinta Feira</td>
<td class=editshade_v style="padding-left:10px" align="left">
{$qui_value}&nbsp;
{END qui_fieldcolumn}
</td></tr>
{BEGIN sex_fieldcolumn}
<tr><td class=headerlist_v width=30% style="padding-left:10px">Sexta Feira</td>
<td class=editshade_v style="padding-left:10px" align="left">
{$sex_value}&nbsp;
{END sex_fieldcolumn}
</td></tr>
{BEGIN sab_fieldcolumn}
<tr><td class=headerlist_v width=30% style="padding-left:10px">Sabado</td>
<td class=editshade_v style="padding-left:10px" align="left">
{$sab_value}&nbsp;
{END sab_fieldcolumn}
</td></tr>
{BEGIN dom_fieldcolumn}
<tr><td class=headerlist_v width=30% style="padding-left:10px">Domingo</td>
<td class=editshade_v style="padding-left:10px" align="left">
{$dom_value}&nbsp;
{END dom_fieldcolumn}
</td></tr>
</table>

{END grid_vrecord}
</td>
{BEGIN endrecord_block}
<td width=20 {$endrecordblock_attrs}>&nbsp;</td>
{END endrecord_block}
{END grid_record}
</tr>
{BEGIN grid_rowspace}
<tr height=30 {$rowspace_attrs}>
{BEGIN grid_recordspace}
<td >&nbsp;</td>
{END grid_recordspace}
</tr>
{END grid_rowspace}
{END grid_row}
</table>
</div>
{END grid_block}
{BEGIN message_block}
<div class="xboxcontentb message_block">
{$message}
</div>
{END message_block}
{BEGIN pagination_block}
<div id="pagination_block{$id}" class="xboxcontentb message_block">
{$pagination}
</div>
{END pagination_block}
</div>
</div>
</div>
</div>
<b class="xbottom"><b class="xb4b"></b><b class="xb3b"></b><b class="xb2b"></b><b class="xb1b"></b></b>
{$footer}
</div>
{END body}
</body>
</html>

