<html {$html_attrs}>
<head><title>Troncos</title>
{BEGIN stylefiles_block}
<link REL="stylesheet" href="include/style.css" type="text/css">
<!--[if IE]>
<link REL="stylesheet" href="include/styleIE.css" type="text/css">
<![endif]-->
{END stylefiles_block}
{BEGIN style_block}
<style>
#height100{$id} {padding:10px;} 
#center_block{$id} {width:500px;margin:0 auto;}
#contents_block{$id} {text-align:center;}
#header_block{$id} {white-space:nowrap;height:31px;padding-top:5px;text-align:center;}
#main_block{$id} {padding:10px 0;text-align:center;}
#inmain_block{$id} {margin:0 10px}
#fields_block{$id} {width:100%}
#buttons_block{$id} {padding:7px 5px 2px;white-space:nowrap;}
#required_block{$id} {text-align:left;padding:5px}
#buttons_block{$id} > * {margin:0 2px}
#message_block{$id} {padding:7px 15px;}
a.pdf{float:left;}
</style>
{END style_block}
</head>
<body>
{BEGIN body}
{$header}
<div id="height100{$id}" align="center">

<table align=center id="center_block{$id}"><tr><td id="contents_block{$id}">
<b class="xtop"><b class="xb1b"></b><b class="xb2b"></b><b class="xb3b"></b><b class="xb4b"></b></b>
<div id="header_block{$id}" class="upeditmenu">
Troncos, Exibir Registro [
Id Tronco: {$show_key1}
]
</div>
<div class="xboxcontentb" id="message_block{$id}">
<br>
<table cellpadding=4 cellspacing=0 border=0 width=470>

{BEGIN id_tronco_fieldblock}
<tr>
    <td class=editshadeleft_b2 width=150 style="padding-left:15px;">Id Tronco</td>
    <td width=250 class=editshaderight_lb2 style="padding-left:10px;">
    {$id_tronco_value}&nbsp;
  </td></tr>
{END id_tronco_fieldblock}
{BEGIN dsc_tronco_fieldblock}
<tr>
    <td class=editshade_b width=150 style="padding-left:15px;">Dsc Tronco</td>
    <td width=250 class=editshade_lb style="padding-left:10px;">
    {$dsc_tronco_value}&nbsp;
  </td></tr>
{END dsc_tronco_fieldblock}
{BEGIN channel_fieldblock}
<tr>
    <td class=editshade_b width=150 style="padding-left:15px;">Channel</td>
    <td width=250 class=editshade_lb style="padding-left:10px;">
    {$channel_value}&nbsp;
  </td></tr>
{END channel_fieldblock}
{BEGIN id_interf_fieldblock}
