<tr>
    <td class=editshade_b width=150 style="padding-left:15px;">Dig Rota</td>
    <td width=250 class=editshade_lb style="padding-left:10px;">
    {$dig_rota_value}&nbsp;
  </td></tr>
{END dig_rota_fieldblock}
{BEGIN id_contrato_fieldblock}
<tr>
    <td class=editshade_b width=150 style="padding-left:15px;">Id Contrato</td>
    <td width=250 class=editshade_lb style="padding-left:10px;">
    {$id_contrato_value}&nbsp;
  </td></tr>
{END id_contrato_fieldblock}
</table>

<div class="downedit2" id="buttons_block{$id}">
{BEGIN prev_button}
 <span class=buttonborder><input class=button type=reset value=" <<< " {$prevbutton_attrs}></span>
{END prev_button}
{BEGIN back_button}
 <span class=buttonborder><input class=button type=reset value="Voltar à Lista" {$backbutton_attrs}></span>
{END back_button}
{BEGIN next_button}
 <span class=buttonborder><input class=button type=reset value=" >>> " {$nextbutton_attrs} ></span>
{END next_button}
&nbsp;
</div>
<b class="xbottom"><b class="xb4a"></b><b class="xb3a"></b><b class="xb2a"></b><b class="xb1a"></b></b>
</div>
<b class="xbottom"><b class="xb4b4"></b><b class="xb3b4"></b><b class="xb2b4"></b><b class="xb1b4"></b></b>
</td></tr></table>
</div>

{BEGIN detail_tables}
{END detail_tables}
<br><br>
{$footer}
{END body}
</body>
</html>
