    <td class=editshade_b width=150 style="padding-left:15px;">Id Interf</td>
    <td width=250 class=editshade_lb style="padding-left:10px;">
    {$id_interf_value}&nbsp;
  </td></tr>
{END id_interf_fieldblock}
{BEGIN usuario_fieldblock}
<tr>
