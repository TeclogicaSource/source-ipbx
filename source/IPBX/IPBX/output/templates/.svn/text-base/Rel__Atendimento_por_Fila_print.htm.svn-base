<html {$html_attrs}>
<head>
<title>Rel. Atendimento por Fila</title>
{BEGIN stylesheetlink}
<link REL="stylesheet" href="include/style.css" type="text/css">
<!--[if IE]>
<link REL="stylesheet" href="include/styleIE.css" type="text/css">
<![endif]-->
{END stylesheetlink}
<style>
.grid_block {  
margin-top:5px;
border-collapse: collapse;
align:center;
width:95%;
border: solid black 1px;
} 
.grid_block td { 
 padding: 5px; 
 margin: 0px; 
 border: solid #cccccc 1px;
}
.grid_block .gridspacing {width:20px;border-top-color:white; border-bottom-color:white;background-color:white}
@media print 
{
	a.pdf {display:none}
}
.printpage {page-break-after:always}
.pagesummary
{
background-color:#ffffbb;
}
.globalsummary
{
background-color:#ffe0b0;
}

{$cssdata}


.group_1
{
background-color: #BCCDDE;
}


.nbgroup_1
{
background-color: #BCCDDE;
border-style:none;
}


.header_1
{
background-color: #9FAEBC;
}


</style> 
</head>
<body topmargin=5 style="background:white">
{BEGIN body}
{$header}
{BEGIN page}
<div>

<h1>Rel. Atendimento por Fila</h1>
<div id='selpage'>Página {$pageno} de {$maxpages} </div>

{BEGIN grid_block}
<table align="center" cellpadding=5 class="grid_block">
{BEGIN grid_header}
<tr>
<td class=blackshade>
{BEGIN dt_entrada_fieldheader}
<b>Data Entrada</b>
{END dt_entrada_fieldheader}
</td>
<td class=blackshade>
{BEGIN Fila_fieldheader}
<b>Fila</b>
{END Fila_fieldheader}
</td>
<td class=blackshade>
{BEGIN hr_entrada_fieldheader}
<b>Hora Entrada</b>
{END hr_entrada_fieldheader}
</td>
<td class=blackshade>
{BEGIN hr_atendimento_fieldheader}
<b>Hora Atendimento</b>
{END hr_atendimento_fieldheader}
</td>
<td class=blackshade>
{BEGIN Telefone_fieldheader}
<b>Telefone</b>
{END Telefone_fieldheader}
</td>
<td class=blackshade>
{BEGIN tp_espera_fieldheader}
<b>Tempo de Espera</b>
{END tp_espera_fieldheader}
</td>
<td class=blackshade>
{BEGIN tp_atendimento_fieldheader}
<b>Tempo de Atendimento</b>
{END tp_atendimento_fieldheader}
</td>
<td class=blackshade>
{BEGIN TMO_fieldheader}
<b>TMO</b>
{END TMO_fieldheader}
</td>
</tr>
{END grid_header}
{BEGIN grid_row}
<!-------------------------------------  group headers ----------------------------------------->
{BEGIN Fila_firstnewgroup}
	<tr>
				<td class=header_1>
		<font style="font-weight:900">{$Fila_grval}</font>
				</td>
<!-------------------------------------  data ----------------------------------------->
	<td class=data>
		{$dt_entrada_value}</td>
	<td class=data>
		{$hr_entrada_value}</td>
	<td class=data>
		{$hr_atendimento_value}</td>
	<td class=data>
		{$Telefone_value}</td>
	<td class=data>
		{$tp_espera_value}</td>
	<td class=data>
		{$tp_atendimento_value}</td>
	<td class=data>
		{$TMO_value}</td>
	</tr>
{END Fila_firstnewgroup}
{BEGIN nonewgroup}
		<td class=group_1>
				&nbsp;
		</td>
	<td class=data>
	{$dt_entrada_value}
	</td>
	<td class=data>
	{$hr_entrada_value}
	</td>
	<td class=data>
	{$hr_atendimento_value}
	</td>
	<td class=data>
	{$Telefone_value}
	</td>
	<td class=data>
	{$tp_espera_value}
	</td>
	<td class=data>
	{$tp_atendimento_value}
	</td>
	<td class=data>
	{$TMO_value}
	</td>
	</tr>
{END nonewgroup}
<!-------------------------------------  group summary ----------------------------------------->
{BEGIN Fila_endgroup}
<tr>
	<td class=group_1
		colspan=8 >
	Resumo para Fila {$Fila_grval}
	 - {$groupFila_total_cnt} Total de registros</div></td>
</tr>
<!--  Avg -->
<tr>
			<td class=group_1
		>&nbsp;</td>
			<td class=group_1
		>Média</td>
			<td class=group_1
		>&nbsp;</td>
			<td class=group_1
		>&nbsp;</td>
			<td class=group_1
		>&nbsp;</td>
			<td class=group_1
		>&nbsp;</td>
			<td class=group_1
		>{$groupFila_totaltp_atendimento_avg}</td>
			<td class=group_1
		>{$groupFila_totalTMO_avg}</td>
</tr>	
<!--  Max -->
<tr>
			<td class=group_1 style="border-bottom: solid black 2px">&nbsp;</td>
			<td class=group_1 style="border-bottom: solid black 2px">Max</td>
			<td class=group_1 style="border-bottom: solid black 2px">&nbsp;</td>
			<td class=group_1 style="border-bottom: solid black 2px">&nbsp;</td>
			<td class=group_1 style="border-bottom: solid black 2px">&nbsp;</td>
			<td class=group_1 style="border-bottom: solid black 2px">&nbsp;</td>
			<td class=group_1 style="border-bottom: solid black 2px">{$groupFila_totaltp_atendimento_max}</td>
			<td class=group_1 style="border-bottom: solid black 2px">{$groupFila_totalTMO_max}</td>
</tr>	
{END Fila_endgroup}
{END grid_row}
</table>
{END grid_block}
</div>
{END page}
{$footer}
{END body}
</body>
</html>
