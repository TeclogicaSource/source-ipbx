<html {$html_attrs}>
<head>
<title>Ramal Analógico</title>
<link REL="stylesheet" href="include/style.css" type="text/css">
<!--[if IE]>
<link REL="stylesheet" href="include/styleIE.css" type="text/css">
<![endif]-->
<style>
.grid_block {  
margin-top:5px;
border-collapse: collapse;
align:center;
width:95%;
border:none;
} 
.grid_block td { 
 padding: 5px; 
 margin: 0px; 
 border: solid black 1px;
}
.grid_block .gridspacing {width:20px;border-top-color:white; border-bottom-color:white;background-color:white}
@media print 
{
	a.pdf {display:none}
}
.printpage {page-break-after:always}
#selpage{margin-left:30px;}
</style> 
</head>
<body style="background-color:white">
{BEGIN body}
{$header}
{BEGIN page}

<h1>Ramal Analógico</h1>
{BEGIN page_number}
<div id='selpage'>Página {$pageno} de {$maxpages} </div>
{END page_number}

{BEGIN grid_block}

<table align="center" cellpadding=5 class="grid_block">
{BEGIN grid_header}
<tr>
{BEGIN record_header}
{BEGIN ramal_fieldheadercolumn}
	<td>
{BEGIN ramal_fieldheader}
	<b>Ramal</b>
{END ramal_fieldheader}
	</td>
{END ramal_fieldheadercolumn}
{BEGIN Nome_fieldheadercolumn}
	<td>
{BEGIN Nome_fieldheader}
	<b>Nome</b>
{END Nome_fieldheader}
	</td>
{END Nome_fieldheadercolumn}
{BEGIN idt_custo_fieldheadercolumn}
	<td>
{BEGIN idt_custo_fieldheader}
	<b>Centro de Custo</b>
{END idt_custo_fieldheader}
	</td>
{END idt_custo_fieldheadercolumn}
{BEGIN context_fieldheadercolumn}
	<td>
{BEGIN context_fieldheader}
	<b>Contexto</b>
{END context_fieldheader}
	</td>
{END context_fieldheadercolumn}
{BEGIN id_horario_fieldheadercolumn}
	<td>
{BEGIN id_horario_fieldheader}
	<b>Horário</b>
{END id_horario_fieldheader}
	</td>
{END id_horario_fieldheadercolumn}
{BEGIN tronco_fieldheadercolumn}
	<td>
{BEGIN tronco_fieldheader}
	<b>Tronco</b>
{END tronco_fieldheader}
	</td>
{END tronco_fieldheadercolumn}
{BEGIN endrecordheader_block}
	<td class="gridspacing">&nbsp;</td>
{END endrecordheader_block}
{END record_header}
</tr>
{END grid_header}
{BEGIN grid_row}
<tr valign=top {$rowstyle} {$rowattrs}>
{BEGIN grid_record}
{BEGIN ramal_fieldcolumn}
<td {$ramal_style}>
{$ramal_value}
</td>
{END ramal_fieldcolumn}
{BEGIN Nome_fieldcolumn}
<td {$Nome_style}>
{$Nome_value}
</td>
{END Nome_fieldcolumn}
{BEGIN idt_custo_fieldcolumn}
<td {$idt_custo_style}>
{$idt_custo_value}
</td>
{END idt_custo_fieldcolumn}
{BEGIN context_fieldcolumn}
<td {$context_style}>
{$context_value}
</td>
{END context_fieldcolumn}
{BEGIN id_horario_fieldcolumn}
<td {$id_horario_style}>
{$id_horario_value}
</td>
{END id_horario_fieldcolumn}
{BEGIN tronco_fieldcolumn}
<td {$tronco_style}>
{$tronco_value}
</td>
{END tronco_fieldcolumn}
{BEGIN endrecord_block}
<td class="gridspacing">&nbsp;</td>
{END endrecord_block}
{END grid_record}
</tr>
{END grid_row}
<!-- totals row -->
</table>
{END grid_block}
{END page}
{$footer}
{END body}
</body>
</html>