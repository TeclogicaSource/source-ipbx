<div class="searchOptions bottomPanelBorder" {$srchOpt_attrs}>
	<!-- Search panel content -->
	<div class="srchCritTop" style="display: none;">
		Critério:&nbsp;
		<input type="radio" name="srchType" class="runner-radio-button" {$all_checkbox}> {BEGIN all_checkbox_label}Todos{END all_checkbox_label}
		&nbsp;&nbsp;&nbsp;
		<input type="radio" name="srchType" class="runner-radio-button" {$any_checkbox}> {BEGIN any_checkbox_label}Qualquer{END any_checkbox_label}
	</div>
	
	<div class="addOption">
		<span class="runner-btnframe">
		<span class="runner-btnleft"></span>
		<span class="runner-btnright"></span>
		<a href="#" id="showHideControlChooseMenu{$id}" class="runner-button" {$showHideCtrls_attrs}>Adicionar campo</a>
		</span>
		&nbsp;&nbsp;&nbsp;
		<span class="runner-btnframe">
		<span class="runner-btnleft"></span>
		<span class="runner-btnright"></span>
		<a href="#" id="showHideSearchType{$id}" class="runner-button" {$showHideCtrlsOpt_attrs}>{$showHideOpt_mess}</a>
		</span>
	</div>
	
	<!-- choose controls div -->
	<div class="runner-panel controlChooseMenu" {$controlChooseMenuDiv_attrs}>
		{BEGIN addSearch_nm_tel1}
			<a href="#" id="addSearchControl_nm_tel1" class="addSearchControl" {$addSearchControl_nm_tel1_attrs}>Número Telefone 1</a><br>
		{END addSearch_nm_tel1}
		{BEGIN addSearch_busca_dt_tel1}
			<a href="#" id="addSearchControl_busca_dt_tel1" class="addSearchControl" {$addSearchControl_busca_dt_tel1_attrs}>Busca por data Telefone 1</a><br>
		{END addSearch_busca_dt_tel1}
		{BEGIN addSearch_nm_tel2}
			<a href="#" id="addSearchControl_nm_tel2" class="addSearchControl" {$addSearchControl_nm_tel2_attrs}>Número Telefone 2</a><br>
		{END addSearch_nm_tel2}
		{BEGIN addSearch_busca_dt_tel2}
			<a href="#" id="addSearchControl_busca_dt_tel2" class="addSearchControl" {$addSearchControl_busca_dt_tel2_attrs}>Busca por data Telefone 2</a><br>
		{END addSearch_busca_dt_tel2}
		{BEGIN addSearch_nm_tel3}
			<a href="#" id="addSearchControl_nm_tel3" class="addSearchControl" {$addSearchControl_nm_tel3_attrs}>Número Telefone 3</a><br>
		{END addSearch_nm_tel3}
		{BEGIN addSearch_busca_dt_tel3}
			<a href="#" id="addSearchControl_busca_dt_tel3" class="addSearchControl" {$addSearchControl_busca_dt_tel3_attrs}>Busca por data Telefone 3</a><br>
		{END addSearch_busca_dt_tel3}
		{BEGIN addSearch_nm_tentativas}
			<a href="#" id="addSearchControl_nm_tentativas" class="addSearchControl" {$addSearchControl_nm_tentativas_attrs}>Número de tentativas</a><br>
		{END addSearch_nm_tentativas}
		{BEGIN addSearch_id_cliente}
			<a href="#" id="addSearchControl_id_cliente" class="addSearchControl" {$addSearchControl_id_cliente_attrs}>TICKET Cliente</a><br>
		{END addSearch_id_cliente}
	</div>
	
	<div class="controlsBlock">
		{BEGIN searchCtrlBlock}
		<div class="srchPanelRow" {$filterRow_attrs}>
			<div class="srchPanelCell">{$delCtrlButt}&nbsp;{$fLabel}:&nbsp;</div>
			<div class="srchPanelCell srchPanelCell2">{$searchtype} {$searchcontrol} {$searchcontrol1}</div>
		</div>
		{END searchCtrlBlock}
	</div>
		
	<div class="bottomSearchButt" align="left" {$bottomSearchButt_attrs}>
		<span class="runner-btnframe">
		<span class="runner-btnleft"></span>
		<span class="runner-btnright"></span>
		<a href="#" class="runner-button" {$searchbutton_attrs}>Buscar</a>
		</span>
	</div>
</div>
