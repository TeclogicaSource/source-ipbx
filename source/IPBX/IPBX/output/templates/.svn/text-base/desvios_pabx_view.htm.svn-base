<html {$html_attrs}>
<head><title>Desvios Telefones Analogicos</title>
<link REL="stylesheet" href="include/style.css" type="text/css">
<!--[if IE]>
<link REL="stylesheet" href="include/styleIE.css" type="text/css">
<![endif]-->
<style>
#height100{$id} {padding:10px;} 
#center_block{$id} {width:500px;margin:0 auto;}
#contents_block{$id} {text-align:center;}
#header_block{$id} {white-space:nowrap;height:31px;padding:5px;text-align:center;}
#footer_block{$id} {height:15px;}
#fields_block{$id} {width:100%}
#buttons_block{$id} {padding:5px;white-space:nowrap;text-align:center;}
#required_block{$id} {text-align:left;padding:5px}
#message_block{$id} {padding:5px 5px;text-align:center;}
a.pdf{float:left;}
</style>
</head>
<body>
{BEGIN body}
{$header}
<div id="height100{$id}" align="center">


<TABLE align=center id="center_block{$id}" cellpadding=0 cellspacing=0><tr><td>

<b class="xtop"><b class="xb1b"></b><b class="xb2b"></b><b class="xb3b"></b><b class="xb4b"></b></b>
<div id="header_block{$id}" class="upeditmenu">
Desvios Telefones Analogicos, Exibir Registro [
Id Desvio: {$show_key1}
]
</div>
<div class="main_table_border2" style="border: 1px solid #516C81;">
<div class="main_table_border">
<TABLE CELLPADDING=0 CELLSPACING=0 align=center border=0 width="480">

{BEGIN id_desvio_fieldblock}
<tr style="padding:3px">
<td class=editshade_b width=150 style="padding-left:15px;">Id Desvio</td>
<td width=250 class=editshade_lb style="padding-left:10px;">
    {$id_desvio_value}&nbsp;
  </td></tr>
{END id_desvio_fieldblock}
{BEGIN ramal_fieldblock}
<tr style="padding:3px">
<td class=editshade_b width=150 style="padding-left:15px;">Ramal</td>
<td width=250 class=editshade_lb style="padding-left:10px;">
    {$ramal_value}&nbsp;
  </td></tr>
{END ramal_fieldblock}
{BEGIN destino_fieldblock}
<tr style="padding:3px">
<td class=editshade_b width=150 style="padding-left:15px;">Destino</td>
<td width=250 class=editshade_lb style="padding-left:10px;">
    {$destino_value}&nbsp;
  </td></tr>
{END destino_fieldblock}
{BEGIN seg_fieldblock}
<tr style="padding:3px">
<td class=editshade_b width=150 style="padding-left:15px;">Segunda Feira</td>
<td width=250 class=editshade_lb style="padding-left:10px;">
    {$seg_value}&nbsp;
  </td></tr>
{END seg_fieldblock}
{BEGIN ter_fieldblock}
<tr style="padding:3px">
<td class=editshade_b width=150 style="padding-left:15px;">Terça Feira</td>
<td width=250 class=editshade_lb style="padding-left:10px;">
    {$ter_value}&nbsp;
  </td></tr>
{END ter_fieldblock}
{BEGIN qua_fieldblock}
<tr style="padding:3px">
<td class=editshade_b width=150 style="padding-left:15px;">Quarta Feira</td>
<td width=250 class=editshade_lb style="padding-left:10px;">
    {$qua_value}&nbsp;
  </td></tr>
{END qua_fieldblock}
{BEGIN qui_fieldblock}
<tr style="padding:3px">
<td class=editshade_b width=150 style="padding-left:15px;">Quinta Feira</td>
<td width=250 class=editshade_lb style="padding-left:10px;">
    {$qui_value}&nbsp;
  </td></tr>
{END qui_fieldblock}
{BEGIN sex_fieldblock}
<tr style="padding:3px">
<td class=editshade_b width=150 style="padding-left:15px;">Sexta Feira</td>
<td width=250 class=editshade_lb style="padding-left:10px;">
    {$sex_value}&nbsp;
  </td></tr>
{END sex_fieldblock}
{BEGIN sab_fieldblock}
<tr style="padding:3px">
<td class=editshade_b width=150 style="padding-left:15px;">Sabado</td>
<td width=250 class=editshade_lb style="padding-left:10px;">
    {$sab_value}&nbsp;
  </td></tr>
{END sab_fieldblock}
{BEGIN dom_fieldblock}
<tr style="padding:3px">
<td class=editshade_b width=150 style="padding-left:15px;">Domingo</td>
<td width=250 class=editshade_lb style="padding-left:10px;">
    {$dom_value}&nbsp;
  </td></tr>
{END dom_fieldblock}
{BEGIN hr_inicio_fieldblock}
<tr style="padding:3px">
<td class=editshade_b width=150 style="padding-left:15px;">Hora Inicio</td>
<td width=250 class=editshade_lb style="padding-left:10px;">
    {$hr_inicio_value}&nbsp;
  </td></tr>
{END hr_inicio_fieldblock}
{BEGIN hr_fim_fieldblock}
<tr style="padding:3px">
<td class=editshade_b width=150 style="padding-left:15px;">Hora Fim</td>
<td width=250 class=editshade_lb style="padding-left:10px;">
    {$hr_fim_value}&nbsp;
  </td></tr>
{END hr_fim_fieldblock}
</table>
</div>
<div class="downedit2" id="buttons_block{$id}">
{BEGIN prev_button}
 <span class=buttonborder><input class=button type=reset value=" <<< " {$prevbutton_attrs}></span>
{END prev_button}
{BEGIN back_button}
<span class=buttonborder><input class=button type=button value="Voltar à Lista" {$backbutton_attrs}></span>
{END back_button}
{BEGIN next_button}
 <span class=buttonborder><input class=button type=reset value=" >>> " {$nextbutton_attrs} ></span>
{END next_button}
</div>


</div>
<b class="xbottom"><b class="xb4b"></b><b class="xb3b"></b><b class="xb2b"></b><b class="xb1b"></b></b>

</td></tr>
</table>
</div>

{BEGIN detail_tables}
{END detail_tables}
<br><br>
{$footer}
{END body}
</body>
</html>
