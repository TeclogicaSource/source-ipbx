<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0 Transitional//EN">
<HTML {$html_attrs}><HEAD><TITLE>Filas</TITLE>
<link REL="stylesheet" href="include/style.css" type="text/css"><!--[if IE]><link REL="stylesheet" href="include/styleIE.css" type="text/css"><![endif]-->{BEGIN style_block}
<STYLE>
#center_block{$id} {width:500px;margin:0 auto;}
#contents_block{$id} {text-align:center;width:100%}
#header_block{$id} {width:100%;white-space:nowrap;height:35px;padding-top:5px;text-align:center;border-bottom:1px white solid;}
#fields_block{$id} {width:100%}
#fields_block{$id} td {padding-left:10px}
#buttons_block{$id} {width:100%;padding-bottom:5px;text-align:center}
#required_block{$id} {text-align:left;padding:5px}
#buttons_block{$id} > * {margin:0 2px;text-align:center}
</STYLE>
{END style_block}
<META name=GENERATOR content="MSHTML 8.00.6001.19222"></HEAD>
<BODY>{BEGIN body}{$header} 
{BEGIN addForm}<TABLE style="WIDTH: 80%" id="center_block{$id}" align=center>
  <TBODY>
  <TR>
    <TD id="contents_block{$id}">{BEGIN flybody}<B class=xtop><B 
      class=xb1b></B><B class=xb2b></B><B class=xb3b></B><B class=xb4b></B></B>
      <DIV id="header_block{$id}" class=upeditmenu>Filas, Novo Registro 
      </DIV>
      {BEGIN message_block}<DIV id="message_block{$id}" class=downedit>{$message}</DIV>{END message_block}
      <TABLE border=0 cellSpacing=0 cellPadding=4 width="100%">
        <TBODY>
        {BEGIN nm_fila_fieldblock}<TR>
          <TD style="PADDING-LEFT: 15px; WIDTH: 285px" class=editshadeleft_b2>{BEGIN nm_fila_label}Nome da fila{END nm_fila_label}</TD>
          <TD style="PADDING-LEFT: 10px" class=editshaderight_lb2 
            width=250>{$nm_fila_editcontrol} </TD></TR>{END nm_fila_fieldblock}
        {BEGIN nm_grupo_fieldblock}<TR>
          <TD style="PADDING-LEFT: 15px" class=editshade_b width=150>Grupo de 
            filas</TD>
          <TD style="PADDING-LEFT: 10px" class=editshade_lb width=250>{$nm_grupo_editcontrol} </TD></TR>{END nm_grupo_fieldblock}
        {BEGIN tp_toques_fieldblock}<TR>
          <TD style="PADDING-LEFT: 15px; WIDTH: 285px" class=editshade_b>Tempo 
            entre toques (segundos)</TD>
          <TD style="PADDING-LEFT: 10px" class=editshade_lb width=250>{$tp_toques_editcontrol} </TD></TR>{END tp_toques_fieldblock}
        {BEGIN estrategia_fieldblock}<TR>
          <TD style="PADDING-LEFT: 15px; WIDTH: 285px" 
            class=editshade_b>Estratégia</TD>
          <TD style="PADDING-LEFT: 10px" class=editshade_lb width=250>{$estrategia_editcontrol} </TD></TR>{END estrategia_fieldblock}
        {BEGIN tp_excedido_fieldblock}<TR>
          <TD style="PADDING-LEFT: 15px; WIDTH: 285px" class=editshade_b>Tempo 
            máximo do usuário em fila (segundos)</TD>
          <TD style="PADDING-LEFT: 10px" class=editshade_lb width=250>{$tp_excedido_editcontrol} </TD></TR>{END tp_excedido_fieldblock}
        {BEGIN acao_tp_excedido_fieldblock}<TR>
          <TD style="PADDING-LEFT: 15px; WIDTH: 285px" 
            class=editshade_b>Dentro do horário e Tempo máximo excedido</TD>
          <TD style="PADDING-LEFT: 10px" class=editshade_lb width=250>{$acao_tp_excedido_editcontrol} </TD></TR>{END acao_tp_excedido_fieldblock}
        {BEGIN acao_falta_agente_fieldblock}<TR>
          <TD style="PADDING-LEFT: 15px; WIDTH: 285px" 
            class=editshade_b>Dentro do horário e falta agente conectado</TD>
          <TD style="PADDING-LEFT: 10px" class=editshade_lb width=250>{$acao_falta_agente_editcontrol} 
</TD></TR>{END acao_falta_agente_fieldblock}
        {BEGIN acao_fr_horario_fieldblock}<TR>
          <TD style="PADDING-LEFT: 15px; WIDTH: 285px" class=editshade_b>Fora 
            do horário de atendimento</TD>
          <TD style="PADDING-LEFT: 10px" class=editshade_lb width=250>{$acao_fr_horario_editcontrol} </TD></TR>{END acao_fr_horario_fieldblock}
        {BEGIN id_desvio_fieldblock}<TR>
          <TD style="PADDING-LEFT: 15px; WIDTH: 285px" 
          class=editshade_b>Desvio</TD>
          <TD style="PADDING-LEFT: 10px" class=editshade_lb width=250>{$id_desvio_editcontrol} </TD></TR>{END id_desvio_fieldblock}
        {BEGIN gravacao_fieldblock}<TR>
          <TD style="PADDING-LEFT: 15px; WIDTH: 285px" 
            class=editshade_b>Gravação</TD>
          <TD style="PADDING-LEFT: 10px" class=editshade_lb width=250>{$gravacao_editcontrol} </TD></TR>{END gravacao_fieldblock}
        {BEGIN arq_audio_fieldblock}<TR>
          <TD style="PADDING-LEFT: 15px; WIDTH: 285px" class=editshade_b>Mens. 
            fora período atend. (WAV)</TD>
          <TD style="PADDING-LEFT: 10px" class=editshade_lb width=250>{$arq_audio_editcontrol} </TD></TR>{END arq_audio_fieldblock}
        {BEGIN arq_mesg_fieldblock}<TR>
          <TD style="PADDING-LEFT: 15px; WIDTH: 285px" class=editshade_b>Mens. 
            antes do atend. (WAV)</TD>
          <TD style="PADDING-LEFT: 10px" class=editshade_lb width=250>{$arq_mesg_editcontrol} </TD></TR>{END arq_mesg_fieldblock}
        {BEGIN id_logout_fieldblock}<TR>
          <TD style="PADDING-LEFT: 15px" class=editshade_b width=150>Deslogar 
            agentes automaticamente</TD>
          <TD style="PADDING-LEFT: 10px" class=editshade_lb width=250>{$id_logout_editcontrol} </TD></TR>{END id_logout_fieldblock}
        {BEGIN maxlen_fieldblock}<TR>
          <TD style="PADDING-LEFT: 15px" class=editshade_b width=150>Tamanho 
            da fila (0 para não especificar)</TD>
          <TD style="PADDING-LEFT: 10px" class=editshade_lb width=250>{$maxlen_editcontrol} </TD></TR>{END maxlen_fieldblock}
        {BEGIN wrapuptime_fieldblock}<TR>
          <TD style="PADDING-LEFT: 15px" class=editshade_b width=150>Tempo em 
            segundos para o atendente entre chamadas (0 para não especificar)</TD>
          <TD style="PADDING-LEFT: 10px" class=editshade_lb width=250>{$wrapuptime_editcontrol} </TD></TR>{END wrapuptime_fieldblock}
        <TR>
          <TD style="BACKGROUND-COLOR: #e6e6e6; WHITE-SPACE: normal" 
            colSpan=2><FONT color=#0000ff>
            <P align=center><FONT color=#0080c0><STRONG><FONT color=#004080 
            size=3>Service Level Agreement - SLA</FONT></STRONG> 
            </FONT></P></FONT></TD></TR>
        {BEGIN seg_tmo_fieldblock}<TR>
          <TD style="PADDING-LEFT: 15px; WIDTH: 285px" class=editshade_b>Tempo 
            máximo da operação </TD>
          <TD style="PADDING-LEFT: 10px" class=editshade_lb width=250>{$seg_tmo_editcontrol} <FONT 
            color=#336699>segundos, maior ou igual</FONT> {$perc_tmo_editcontrol}&nbsp;<FONT 
            color=#336699>%</FONT></TD></TR>{END seg_tmo_fieldblock}
        {BEGIN seg_tma_fieldblock}<TR>
          <TD style="PADDING-LEFT: 15px; WIDTH: 285px" class=editshade_b>Tempo 
            máximo do atendimento </TD>
          <TD style="PADDING-LEFT: 10px" class=editshade_lb width=250>{$seg_tma_editcontrol} <FONT 
            color=#336699>segundos, maior ou igual </FONT>{$perc_tma_editcontrol}<FONT 
            color=#336699>%</FONT></TD></TR>{END seg_tma_fieldblock}
        {BEGIN seg_tme_fieldblock}<TR>
          <TD style="PADDING-LEFT: 15px; WIDTH: 285px" class=editshade_b>Tempo 
            máximo de espera</TD>
          <TD style="PADDING-LEFT: 10px" class=editshade_lb width=250>{$seg_tme_editcontrol}<FONT color=#336699> 
            segundos, maior ou igual</FONT> {$perc_tme_editcontrol}<FONT 
            color=#336699>%</FONT></TD></TR>{END seg_tme_fieldblock}
        {BEGIN tx_abandono_fieldblock}<TR>
          <TD style="PADDING-LEFT: 15px; WIDTH: 285px" class=editshade_b>Taxa 
            de Abandono (Percentual)</TD>
          <TD style="PADDING-LEFT: 10px" class=editshade_lb width=250>{$tx_abandono_editcontrol} <FONT 
            color=#336699>%</FONT></TD></TR>{END tx_abandono_fieldblock}
        {BEGIN id_name_gestor_fieldblock}<TR>
          <TD style="PADDING-LEFT: 15px; WIDTH: 285px" 
            class=editshade_b>Gestor da fila</TD>
          <TD style="PADDING-LEFT: 10px" class=editshade_lb width=250>
            <P align=left><FONT color=#336699>Estimativa do dia</FONT>{$flg_estim_do_dia_editcontrol}&nbsp;<FONT 
            color=#afeeee></FONT><FONT color=#336699>ou informe o Tempo 
            (Min.)</FONT> {$tp_estimativa_editcontrol}&nbsp;</P>
            <P>{$id_name_gestor_editcontrol} 
        </P></TD></TR>{END id_name_gestor_fieldblock}</TBODY></TABLE>
      <DIV id="buttons_block{$id}" class=downedit2>
      {BEGIN legend}<DIV id="required_block{$id}"><IMG alt=bullet 
      src="images/icon_required.gif"> - Required field</DIV>{END legend}{$legendBreak} {BEGIN save_button}<SPAN class=buttonborder><INPUT class=button value=Salvar type=submit {$savebutton_attrs}></SPAN>{END save_button} 
      {BEGIN back_button}<SPAN class=buttonborder><INPUT class=button value="Voltar à Lista" type=button {$backbutton_attrs}></SPAN>{END back_button} 
      {BEGIN cancel_button}<SPAN class=buttonborder><INPUT class=button value=Cancela type=button {$cancelbutton_attrs}></SPAN>{END cancel_button} 
      </DIV><B class=xbottom><B class=xb4a></B><B class=xb3a></B><B 
      class=xb2a></B><B class=xb1a></B></B>{END flybody}</TD></TR></TBODY></TABLE>{END addForm}{BEGIN detail_tables}{END detail_tables}<BR><BR>{$footer} {END body}</BODY></HTML>