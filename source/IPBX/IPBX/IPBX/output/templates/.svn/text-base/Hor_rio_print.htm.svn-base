<html {$html_attrs}>
<head>
<title>Horarios</title>
{BEGIN stylesheetlink}
<link REL="stylesheet" href="include/style.css" type="text/css">
<!--[if IE]>
<link REL="stylesheet" href="include/styleIE.css" type="text/css">
<![endif]-->
{END stylesheetlink}
<style>
.grid_block {  
margin-top:5px;
border-collapse: collapse;
align:center;
width:95%;
border: solid black 1px;
} 
.grid_block td { 
 padding: 5px; 
 margin: 0px; 
 border: solid #cccccc 1px;
}
.grid_block .gridspacing {width:20px;border-top-color:white; border-bottom-color:white;background-color:white}
@media print 
{
	a.pdf {display:none}
}
.printpage {page-break-after:always}

.pagesummary
{
background-color:#ffffbb;
}
.globalsummary
{
background-color:#ffe0b0;
}

{$cssdata}

</style> 
</head>
<body topmargin=5 style="background:white">
{BEGIN body}
{$header}
{BEGIN page}
<div>

<h1>Horarios</h1>
<div id='selpage'>Página {$pageno} de {$maxpages} </div>

{BEGIN grid_block}
<table align="center" cellpadding=5 class="grid_block">
{BEGIN grid_header}
<tr>
<td class=blackshade>
{BEGIN idt_agenda_fieldheader}
<b>Idt Agenda</b>
{END idt_agenda_fieldheader}
</td>
<td class=blackshade>
{BEGIN Nome_fieldheader}
<b>Nome</b>
{END Nome_fieldheader}
</td>
<td class=blackshade>
{BEGIN Numero_fieldheader}
<b>Numero</b>
{END Numero_fieldheader}
</td>
<td class=blackshade>
{BEGIN idt_custo_fieldheader}
<b>Idt Custo</b>
{END idt_custo_fieldheader}
</td>
</tr>
{END grid_header}
{BEGIN grid_row}
<!-------------------------------------  data ----------------------------------------->
{BEGIN row_data}
<tr>
	<td class=data>{$idt_agenda_value}</td>
	<td class=data>{$Nome_value}</td>
	<td class=data>{$Numero_value}</td>
	<td class=data>{$idt_custo_value}</td>
</tr>
{END row_data}
{END grid_row}
</table>
{END grid_block}
</div>
{END page}
{$footer}
{END body}
</body>
</html>