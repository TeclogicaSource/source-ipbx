<html {$html_attrs}>
<head>
<title>Vono: Busca Avancada</title>
<link REL="stylesheet" href="include/style.css" type="text/css">
<!--[if IE]>
<link REL="stylesheet" href="include/styleIE.css" type="text/css">
<![endif]-->
<style>
#center_block{$id} {margin:0 auto;}
#contents_block{$id} {text-align:center;width:100%}
#header_block{$id} {white-space:nowrap;height:31px;padding-top:5px;}
#main_block{$id} {padding:10px 0;width:100%}
#inmain_block{$id} {margin:0 10px}
#buttons_block{$id} {width:100%;padding:10px 0;}
#buttons_block{$id} > * {margin:0 2px}
#conditions_block{$id} {padding:5px;}
</style>
<!--[if IE]>
<style>
#main_block{$id} {padding:10px 10;}
#inmain_block{$id} {margin:0 0px}
</style>
<![endif]-->
</head>
<body bgcolor=white>
{BEGIN body}
{$header}
{BEGIN contents_block}
<table align=center id="center_block{$id}"><tr><td id="contents_block{$id}">
	
<b class="xtop"><b class="xb1b"></b><b class="xb2b"></b><b class="xb3b"></b><b class="xb4b"></b></b>
<div id="header_block{$id}" class="upeditmenu">Vono - Busca Avancada</div>

<div class="main_table_border2">
<div class="main_table_border">

{BEGIN conditions_block}
<DIV id="conditions_block{$id}" class="loginshade">
<span class=fieldname>Buscar:</span>
<input type="radio" name="srchType" {$all_checkbox}>{BEGIN all_checkbox_label}Todas condições{END all_checkbox_label}
&nbsp;&nbsp;&nbsp;
<input type="radio" name="srchType" {$any_checkbox}>{BEGIN any_checkbox_label}Qualquer condição{END any_checkbox_label}
</div>
{END conditions_block}
<table cellpadding=4 cellspacing=0 border=0 id="fields_block{$id}">
<tr valign=center>
<td align=center valign=middle class="headerlist">&nbsp;</td>
<td width=30 align=center valign=middle class=headerlist>NOT</td>
<td align=center valign=middle class="headerlist">&nbsp; </td>
<td align=center valign=middle class="headerlist">&nbsp; </td>
<td align=center valign=middle class="headerlist" style="border-right:0px;">&nbsp;</td></tr>

{BEGIN id_vono_fieldblock}	
<tr>
	<td style="padding-left:15px;" class="editshade_b"><b>{BEGIN id_vono_label}Id Vono{END id_vono_label}</b>
	<td align=center style="padding-left:10px;" class="editshade_lb"><input type=checkbox {$id_vono_notbox}></td>
	
	<td style="padding-left:10px;" class="editshade_lb">
	{$searchtype_id_vono}
	</td>
	
	<td width=270 style="padding-left:10px;" class="editshade_lb">{$id_vono_editcontrol}&nbsp;</td>

	<td width=270 style="padding-left:10px;" class="editshade_lb">{$id_vono_editcontrol1}&nbsp;</td>
</tr>
{END id_vono_fieldblock}
{BEGIN CODLOCALIDADE_fieldblock}	
<tr>
	<td style="padding-left:15px;" class="editshade_b"><b>{BEGIN CODLOCALIDADE_label}CODLOCALIDADE{END CODLOCALIDADE_label}</b>
	<td align=center style="padding-left:10px;" class="editshade_lb"><input type=checkbox {$CODLOCALIDADE_notbox}></td>
	
	<td style="padding-left:10px;" class="editshade_lb">
	{$searchtype_CODLOCALIDADE}
	</td>
	
	<td width=270 style="padding-left:10px;" class="editshade_lb">{$CODLOCALIDADE_editcontrol}&nbsp;</td>

	<td width=270 style="padding-left:10px;" class="editshade_lb">{$CODLOCALIDADE_editcontrol1}&nbsp;</td>
</tr>
{END CODLOCALIDADE_fieldblock}
{BEGIN LOCALIDADE_fieldblock}	
<tr>
	<td style="padding-left:15px;" class="editshade_b"><b>{BEGIN LOCALIDADE_label}LOCALIDADE{END LOCALIDADE_label}</b>
	<td align=center style="padding-left:10px;" class="editshade_lb"><input type=checkbox {$LOCALIDADE_notbox}></td>
	
	<td style="padding-left:10px;" class="editshade_lb">
	{$searchtype_LOCALIDADE}
	</td>
	
	<td width=270 style="padding-left:10px;" class="editshade_lb">{$LOCALIDADE_editcontrol}&nbsp;</td>

	<td width=270 style="padding-left:10px;" class="editshade_lb">{$LOCALIDADE_editcontrol1}&nbsp;</td>
</tr>
{END LOCALIDADE_fieldblock}
{BEGIN MUNICIPIO_fieldblock}	
<tr>
	<td style="padding-left:15px;" class="editshade_b"><b>{BEGIN MUNICIPIO_label}MUNICIPIO{END MUNICIPIO_label}</b>
	<td align=center style="padding-left:10px;" class="editshade_lb"><input type=checkbox {$MUNICIPIO_notbox}></td>
	
	<td style="padding-left:10px;" class="editshade_lb">
	{$searchtype_MUNICIPIO}
	</td>
	
	<td width=270 style="padding-left:10px;" class="editshade_lb">{$MUNICIPIO_editcontrol}&nbsp;</td>

	<td width=270 style="padding-left:10px;" class="editshade_lb">{$MUNICIPIO_editcontrol1}&nbsp;</td>
</tr>
{END MUNICIPIO_fieldblock}
{BEGIN ESTADO_fieldblock}	
<tr>
	<td style="padding-left:15px;" class="editshade_b"><b>{BEGIN ESTADO_label}ESTADO{END ESTADO_label}</b>
	<td align=center style="padding-left:10px;" class="editshade_lb"><input type=checkbox {$ESTADO_notbox}></td>
	
	<td style="padding-left:10px;" class="editshade_lb">
	{$searchtype_ESTADO}
	</td>
	
	<td width=270 style="padding-left:10px;" class="editshade_lb">{$ESTADO_editcontrol}&nbsp;</td>

	<td width=270 style="padding-left:10px;" class="editshade_lb">{$ESTADO_editcontrol1}&nbsp;</td>
</tr>
{END ESTADO_fieldblock}
{BEGIN PREFIXO_fieldblock}	
<tr>
	<td style="padding-left:15px;" class="editshade_b"><b>{BEGIN PREFIXO_label}PREFIXO{END PREFIXO_label}</b>
	<td align=center style="padding-left:10px;" class="editshade_lb"><input type=checkbox {$PREFIXO_notbox}></td>
	
	<td style="padding-left:10px;" class="editshade_lb">
	{$searchtype_PREFIXO}
	</td>
	
	<td width=270 style="padding-left:10px;" class="editshade_lb">{$PREFIXO_editcontrol}&nbsp;</td>

	<td width=270 style="padding-left:10px;" class="editshade_lb">{$PREFIXO_editcontrol1}&nbsp;</td>
</tr>
{END PREFIXO_fieldblock}
</table>

<div class="downedit" id="buttons_block{$id}">
{BEGIN search_button}
 <span class=buttonborder><input class=button type=button value="Buscar" {$searchbutton_attrs}></span>
{END search_button} 
{BEGIN reset_button}
 <span class=buttonborder><input class=button type=button value="Limpar" {$resetbutton_attrs}></span>
{END reset_button}
{BEGIN back_button}
 <span class=buttonborder><input class=button type=button value="Voltar à Lista" {$backbutton_attrs}></span>
{END back_button}
</div>
</div>
</div>
<b class="xbottom"><b class="xb4b"></b><b class="xb3b"></b><b class="xb2b"></b><b class="xb1b"></b></b>
</td></tr></table>
{END contents_block}
{$footer}
{END body}
</body>
</html>