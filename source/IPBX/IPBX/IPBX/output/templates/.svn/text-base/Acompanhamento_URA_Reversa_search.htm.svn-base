<html {$html_attrs}>
<head>
<title>URA Reversa: Busca Avancada</title>
<link REL="stylesheet" href="include/style.css" type="text/css">
<!--[if IE]>
<link REL="stylesheet" href="include/styleIE.css" type="text/css">
<![endif]-->
<style>
#center_block{$id} {margin:0 auto;}
#contents_block{$id} {text-align:center;}
#header_block{$id} {white-space:nowrap;height:31px;padding-top:5px;border-bottom:1px solid white;}
#main_block{$id} {padding:10px 10px;}
#buttons_block{$id} {padding:10px 0 5px 0;}
#buttons_block{$id} > * {margin:0 2px}
</style>
<!--[if IE]>
<style>
#main_block{$id} {padding:10px 10;}
#inmain_block{$id} {margin:0 0px}
</style>
<![endif]-->
</head>
<body bgcolor=white>
{BEGIN body}
{$header}
{BEGIN contents_block}
<table align=center id="center_block{$id}"><tr><td id="contents_block{$id}">
<b class="xtop"><b class="xb1b"></b><b class="xb2b"></b><b class="xb3b"></b><b class="xb4b"></b></b>
<div id="header_block{$id}" class="tableheader">URA Reversa - Busca Avancada</div>
<div class="xboxcontentb" id="main_block{$id}">

{BEGIN conditions_block}
<DIV id="conditions_block{$id}">
<b class="xtop"><b class="xb1"></b><b class="xb2"></b><b class="xb3"></b><b class="xb4"></b></b>
<div class="xboxcontent" style="width:100%">
<span class=fieldname>Buscar:</span>
<input type="radio" name="srchType"" {$all_checkbox}>{BEGIN all_checkbox_label}Todas condições{END all_checkbox_label}
&nbsp;&nbsp;&nbsp;
<input type="radio" name="srchType" {$any_checkbox}>{BEGIN any_checkbox_label}Qualquer condição{END any_checkbox_label}
</div>
<b class="xbottom"><b class="xb4"></b><b class="xb3"></b><b class="xb2"></b><b class="xb1"></b></b>
<br>
</div>
{END conditions_block}

<table cellpadding=4 cellspacing=0 id="fields_block">
<tr valign=center>
<td align=center valign=middle class="headerlist_left">&nbsp;</td>
<td width=30 align=center valign=middle class="headerlist2">NOT</td>
<td align=center valign=middle class="headerlist">&nbsp; </td>
<td align=center valign=middle class="headerlist">&nbsp; </td>
<td align=center valign=middle class="headerlist_right">&nbsp;</td></tr>

{BEGIN id_ura_rev_fieldblock}			
<tr>
	<td class=editshade_b style="padding-left:15px;padding-right:10px;">Id Ura Rev
	<td align=center class=editshade_lb><input type=CheckBox {$id_ura_rev_notbox}></td>
	<td class=editshade_lb style="padding-left:10px;">{$searchtype_id_ura_rev}</td>
	<td width=270 class=editshade_lb style="padding-left:10px;">{$id_ura_rev_editcontrol}&nbsp;</td>
	<td width=270 class=editshade_lb style="padding-left:10px;">{$id_ura_rev_editcontrol1}&nbsp;</td>
</tr>
{END id_ura_rev_fieldblock}	
{BEGIN nm_telefone_fieldblock}			
<tr>
	<td class=editshade_b style="padding-left:15px;padding-right:10px;">Número Telefone
	<td align=center class=editshade_lb><input type=CheckBox {$nm_telefone_notbox}></td>
	<td class=editshade_lb style="padding-left:10px;">{$searchtype_nm_telefone}</td>
	<td width=270 class=editshade_lb style="padding-left:10px;">{$nm_telefone_editcontrol}&nbsp;</td>
	<td width=270 class=editshade_lb style="padding-left:10px;">{$nm_telefone_editcontrol1}&nbsp;</td>
</tr>
{END nm_telefone_fieldblock}	
{BEGIN id_msg_fieldblock}			
<tr>
	<td class=editshade_b style="padding-left:15px;padding-right:10px;">Mensagem
	<td align=center class=editshade_lb><input type=CheckBox {$id_msg_notbox}></td>
	<td class=editshade_lb style="padding-left:10px;">{$searchtype_id_msg}</td>
	<td width=270 class=editshade_lb style="padding-left:10px;">{$id_msg_editcontrol}&nbsp;</td>
	<td width=270 class=editshade_lb style="padding-left:10px;">{$id_msg_editcontrol1}&nbsp;</td>
</tr>
{END id_msg_fieldblock}	
{BEGIN dt_criado_fieldblock}			
<tr>
	<td class=editshade_b style="padding-left:15px;padding-right:10px;">Data Criado
	<td align=center class=editshade_lb><input type=CheckBox {$dt_criado_notbox}></td>
	<td class=editshade_lb style="padding-left:10px;">{$searchtype_dt_criado}</td>
	<td width=270 class=editshade_lb style="padding-left:10px;">{$dt_criado_editcontrol}&nbsp;</td>
	<td width=270 class=editshade_lb style="padding-left:10px;">{$dt_criado_editcontrol1}&nbsp;</td>
</tr>
{END dt_criado_fieldblock}	
{BEGIN nm_tentativas_fieldblock}			
<tr>
	<td class=editshade_b style="padding-left:15px;padding-right:10px;">Número de Tentat.
	<td align=center class=editshade_lb><input type=CheckBox {$nm_tentativas_notbox}></td>
	<td class=editshade_lb style="padding-left:10px;">{$searchtype_nm_tentativas}</td>
	<td width=270 class=editshade_lb style="padding-left:10px;">{$nm_tentativas_editcontrol}&nbsp;</td>
	<td width=270 class=editshade_lb style="padding-left:10px;">{$nm_tentativas_editcontrol1}&nbsp;</td>
</tr>
{END nm_tentativas_fieldblock}	
{BEGIN ult_status_fieldblock}			
<tr>
	<td class=editshade_b style="padding-left:15px;padding-right:10px;">Status
	<td align=center class=editshade_lb><input type=CheckBox {$ult_status_notbox}></td>
	<td class=editshade_lb style="padding-left:10px;">{$searchtype_ult_status}</td>
	<td width=270 class=editshade_lb style="padding-left:10px;">{$ult_status_editcontrol}&nbsp;</td>
	<td width=270 class=editshade_lb style="padding-left:10px;">{$ult_status_editcontrol1}&nbsp;</td>
</tr>
{END ult_status_fieldblock}	
{BEGIN resp_usuario_fieldblock}			
<tr>
	<td class=editshade_b style="padding-left:15px;padding-right:10px;">Resposta Usuário
	<td align=center class=editshade_lb><input type=CheckBox {$resp_usuario_notbox}></td>
	<td class=editshade_lb style="padding-left:10px;">{$searchtype_resp_usuario}</td>
	<td width=270 class=editshade_lb style="padding-left:10px;">{$resp_usuario_editcontrol}&nbsp;</td>
	<td width=270 class=editshade_lb style="padding-left:10px;">{$resp_usuario_editcontrol1}&nbsp;</td>
</tr>
{END resp_usuario_fieldblock}	
{BEGIN dt_resp_fieldblock}			
<tr>
	<td class=editshade_b style="padding-left:15px;padding-right:10px;">Data Resposta
	<td align=center class=editshade_lb><input type=CheckBox {$dt_resp_notbox}></td>
	<td class=editshade_lb style="padding-left:10px;">{$searchtype_dt_resp}</td>
	<td width=270 class=editshade_lb style="padding-left:10px;">{$dt_resp_editcontrol}&nbsp;</td>
	<td width=270 class=editshade_lb style="padding-left:10px;">{$dt_resp_editcontrol1}&nbsp;</td>
</tr>
{END dt_resp_fieldblock}	
{BEGIN id_cliente_fieldblock}			
<tr>
	<td class=editshade_b style="padding-left:15px;padding-right:10px;">Cliente
	<td align=center class=editshade_lb><input type=CheckBox {$id_cliente_notbox}></td>
	<td class=editshade_lb style="padding-left:10px;">{$searchtype_id_cliente}</td>
	<td width=270 class=editshade_lb style="padding-left:10px;">{$id_cliente_editcontrol}&nbsp;</td>
	<td width=270 class=editshade_lb style="padding-left:10px;">{$id_cliente_editcontrol1}&nbsp;</td>
</tr>
{END id_cliente_fieldblock}	
</table>

<div class="downedit" id="buttons_block{$id}">
{BEGIN search_button}
 <span class=buttonborder><input class=button type=button value="Buscar" {$searchbutton_attrs}></span>
{END search_button} 
{BEGIN reset_button}
 <span class=buttonborder><input class=button type=button value="Limpar" {$resetbutton_attrs}></span>
{END reset_button}
{BEGIN back_button}
 <span class=buttonborder><input class=button type=button value="Voltar à Lista" {$backbutton_attrs}></span>
{END back_button}
</div>
<b class="xbottom"><b class="xb4a"></b><b class="xb3a"></b><b class="xb2a"></b><b class="xb1a"></b></b>
</div>
<b class="xbottom"><b class="xb4b4"></b><b class="xb3b4"></b><b class="xb2b4"></b><b class="xb1b4"></b></b>
</td></tr></table>
{END contents_block}
{$footer}
{END body}
</body>
</html>
