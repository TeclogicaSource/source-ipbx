<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0 Transitional//EN">
<HTML {$html_attrs}><HEAD><TITLE>Cham Detalhada</TITLE>
{BEGIN stylesheetlink}<link REL="stylesheet" href="include/style.css" type="text/css"><!--[if IE]><link REL="stylesheet" href="include/styleIE.css" type="text/css"><![endif]-->{END stylesheetlink}
<STYLE>
.grid_block {  
margin-top:5px;
border-collapse: collapse;
align:center;
width:95%;
border: solid black 1px;
} 
.grid_block td { 
 padding: 5px; 
 margin: 0px; 
 border: solid #cccccc 1px;
}
.grid_block .gridspacing {width:20px;border-top-color:white; border-bottom-color:white;background-color:white}
@media print 
{
	a.pdf {display:none}
}
.printpage {page-break-after:always}

.pagesummary
{
background-color:#ffffbb;
}
.globalsummary
{
background-color:#ffe0b0;
}

{$cssdata}

</STYLE>

<META name=GENERATOR content="MSHTML 8.00.6001.19222"></HEAD>
<BODY style="BACKGROUND: white" topMargin=5>{BEGIN body}{$header} 
{BEGIN page}<H1>Relatório de Chamadas Detalhadas</H1>{END page}
{BEGIN page}<DIV id=selpage>Página {$pageno} de {$maxpages} </DIV>{END page}
{BEGIN page}<DIV>{BEGIN grid_block}
<TABLE class=grid_block cellPadding=5 align=center>
  <TBODY>
  {BEGIN grid_header}<TR>
    <TD class=blackshade>{BEGIN dt_entrada_fieldheader}<B>Data Entrada</B> 
    {END dt_entrada_fieldheader}</TD>
    <TD class=blackshade>{BEGIN hr_entrada_fieldheader}<B>Hora Entrada</B> 
    {END hr_entrada_fieldheader}</TD>
    <TD class=blackshade>{BEGIN Entrada_Saida_fieldheader}<B>Entrada/Saida</B> {END Entrada_Saida_fieldheader}</TD>
    <TD class=blackshade>{BEGIN Fila_fieldheader}<B>Fila</B> {END Fila_fieldheader}</TD>
    <TD class=blackshade>{BEGIN Telefone_fieldheader}<B>Telefone</B> {END Telefone_fieldheader}</TD>
    <TD class=blackshade>{BEGIN Terminado_fieldheader}<B>Terminado</B> {END Terminado_fieldheader}</TD>
    <TD class=blackshade>{BEGIN Agente_fieldheader}<B>Agente</B> {END Agente_fieldheader}</TD>
    <TD class=blackshade>{BEGIN tp_espera_fieldheader}<B>Espera</B> {END tp_espera_fieldheader}</TD>
    <TD class=blackshade>{BEGIN tp_atendimento_fieldheader}<B>Atendimento</B> {END tp_atendimento_fieldheader}</TD>
    <TD class=blackshade>{BEGIN tptotal_fieldheader}<B>Total</B> {END tptotal_fieldheader}</TD>
    <TD class=blackshade>{BEGIN Obs__fieldheader}<B>Observação</B> {END Obs__fieldheader}</TD></TR>{END grid_header}{BEGIN grid_row}<!-------------------------------------  data ----------------------------------------->
  {BEGIN row_data}<TR>
    <TD class=data>
      <P align=center>{$dt_entrada_value}</P></TD>
    <TD class=data>
      <P align=center>{$hr_entrada_value}</P></TD>
    <TD class=data>
      <P align=center>{$Entrada_Saida_value}</P></TD>
    <TD class=data>
      <P align=center>{$Fila_value}</P></TD>
    <TD class=data>{$Telefone_value}</TD>
    <TD class=data>
      <P align=center>{$Terminado_value}</P></TD>
    <TD class=data>
      <P align=left>{$Agente_value}</P></TD>
    <TD class=data>
      <P align=center>{$tp_espera_value}</P></TD>
    <TD class=data>
      <P align=center>{$tp_atendimento_value}</P></TD>
    <TD class=data>
      <P align=center>{$tptotal_value}</P></TD>
    <TD class=data>
      <P align=left>{$Obs__value}</P></TD></TR>{END row_data}{END grid_row}</TBODY></TABLE>{END grid_block}</DIV>{END page}{$footer} {END body}</BODY></HTML>