<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0 Transitional//EN">
<HTML {$html_attrs}><HEAD><TITLE>Agentes</TITLE>
<link REL="stylesheet" href="include/style.css" type="text/css"><!--[if IE]><link REL="stylesheet" href="include/styleIE.css" type="text/css"><![endif]-->
<STYLE>
#center_block{$id} {width:500px;margin:0 auto;}
#contents_block{$id} {text-align:center;width:100%}
#header_block{$id} {width:100%;white-space:nowrap;height:35px;padding-top:5px;text-align:center;border-bottom:1px white solid;}
#fields_block{$id} {width:100%}
#fields_block{$id} td {padding-left:10px}
#buttons_block{$id} {width:100%;padding-bottom:5px;text-align:center}
#required_block{$id} {text-align:left;padding:5px}
#buttons_block{$id} > * {margin:0 2px;text-align:center}
#span{$id} {margin:0 20px 0 20px;}
</STYLE>

<META name=GENERATOR content="MSHTML 8.00.6001.19222"></HEAD>
<BODY>{BEGIN body}{$header} 
{BEGIN editForm}<TABLE style="WIDTH: 1031px; HEIGHT: 349px" id="center_block{$id}" align=center><TR><TD id="contents_block{$id}"><B class=xtop><B class=xb1b></B><B 
      class=xb2b></B><B class=xb3b></B><B class=xb4b></B></B>
      <DIV id="header_block{$id}" class=upeditmenu>Agentes, Editar registro 
      [ Identificação: {$show_key1} ] </DIV>
      {BEGIN message_block}<DIV id="message_block{$id}" class=downedit>{$message}</DIV>{END message_block}
      <TABLE border=0 cellSpacing=0 cellPadding=4 width="100%">{BEGIN flg_ativo_fieldblock}<TR><td class=editshade_b width=150 style="padding-left:15px;">Flg Ativo</td>
          <td width=250 class=editshade_lb style="padding-left:10px;">{$flg_ativo_editcontrol} </td></TR>
{END flg_ativo_fieldblock}
        {BEGIN nm_agente_fieldblock}<TR><TD style="PADDING-LEFT: 15px" class=editshade_b width=150>Nome de 
            Agente </TD>
          <TD style="PADDING-LEFT: 10px" class=editshade_lb width=250>{$nm_agente_editcontrol} </TD></TR>{END nm_agente_fieldblock}
        {BEGIN name_fieldblock}<TR><TD style="PADDING-LEFT: 15px; WIDTH: 194px" 
          class=editshade_b>Ramal</TD>
          <TD style="PADDING-LEFT: 10px" class=editshade_lb width=250>{$name_editcontrol} </TD></TR>{END name_fieldblock}{BEGIN flg_ativo_fieldblock}<TR><td class=editshade_b width=150 style="padding-left:15px;">Flg Ativo</td>
          <td width=250 class=editshade_lb style="padding-left:10px;">{$flg_ativo_editcontrol} </td></TR>
{END flg_ativo_fieldblock}
</TABLE>
      <DIV id="buttons_block{$id}" class=downedit2>{BEGIN legend}<DIV id="required_block{$id}"><IMG alt=bullet 
      src="images/icon_required.gif"> - Required field</DIV>{END legend}{$legendBreak} {BEGIN save_button}<SPAN class=buttonborder><INPUT id="savebutton{$id}" class=button value=Salvar type=submit {$savebutton_attrs}></SPAN>{END save_button} 
      {BEGIN reset_button}<SPAN class=buttonborder><INPUT class=button value=Limpar type=reset {$resetbutton_attrs}></SPAN>{END reset_button} 
      <SPAN id="span{$id}">&nbsp;</SPAN> {BEGIN prev_button}<SPAN class=buttonborder><INPUT id="prev{$id}" class=button value=" <<< " type=reset {$prevbutton_attrs}></SPAN>{END prev_button} 
      {BEGIN back_button}<SPAN class=buttonborder><INPUT class=button value="Voltar à Lista" type=button {$backbutton_attrs}></SPAN>{END back_button} 
      {BEGIN next_button}<SPAN class=buttonborder><INPUT id="next{$id}" class=button value=" >>> " type=reset {$nextbutton_attrs}></SPAN>{END next_button} 
      </DIV><B class=xbottom><B class=xb4a></B><B class=xb3a></B><B 
      class=xb2a></B><B class=xb1a></B></B></TD></TR></TABLE>{END editForm}{BEGIN detail_tables}{$displayDetailTable_cc_agentes_fila} {END detail_tables}<BR><BR>{$footer} {END body}</BODY></HTML>