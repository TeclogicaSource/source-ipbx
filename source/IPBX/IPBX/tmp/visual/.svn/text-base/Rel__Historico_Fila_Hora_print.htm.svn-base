<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0 Transitional//EN">
<HTML {$html_attrs}><HEAD><TITLE>Rel. Historico Fila Hora</TITLE>
{BEGIN stylesheetlink}<link REL="stylesheet" href="include/style.css" type="text/css"><!--[if IE]><link REL="stylesheet" href="include/styleIE.css" type="text/css"><![endif]-->{END stylesheetlink}
<STYLE>
.grid_block {  
margin-top:5px;
border-collapse: collapse;
align:center;
width:95%;
border: solid black 1px;
} 
.grid_block td { 
 padding: 5px; 
 margin: 0px; 
 border: solid #cccccc 1px;
}
.grid_block .gridspacing {width:20px;border-top-color:white; border-bottom-color:white;background-color:white}
@media print 
{
	a.pdf {display:none}
}
.printpage {page-break-after:always}

.pagesummary
{
background-color:#ffffbb;
}
.globalsummary
{
background-color:#ffe0b0;
}

{$cssdata}

</STYLE>

<META name=GENERATOR content="MSHTML 8.00.6001.19222"></HEAD>
<BODY style="BACKGROUND: white" topMargin=5>{BEGIN body}{$header} 
{BEGIN page}<H1>Relatório Historico Fila Hora</H1>{END page}
{BEGIN page}<DIV id=selpage>Página {$pageno} de {$maxpages} </DIV>{END page}
{BEGIN page}<DIV>{BEGIN grid_block}
<TABLE class=grid_block cellPadding=5 align=center>
  <TBODY>
  {BEGIN grid_header}<TR>
    <TD class=blackshade>{BEGIN Fila_fieldheader}
      <P align=center><B>Fila</B> </P>{END Fila_fieldheader}</TD>
    <TD class=blackshade>{BEGIN Hora_fieldheader}
      <P align=center><B>Hora</B> </P>{END Hora_fieldheader}</TD>
    <TD class=blackshade>{BEGIN Agent_Disp_fieldheader}
      <P align=center><B>Média de Agentes Disponíveis</B> </P>{END Agent_Disp_fieldheader}</TD>
    <TD class=blackshade>{BEGIN AgentesPausa_fieldheader}
      <P align=center><B>Média de Agentes Pausa</B> </P>{END AgentesPausa_fieldheader}</TD>
    <TD class=blackshade>{BEGIN Chamadas_Atendidas_fieldheader}
      <P align=center><B>Total de chamadas atendidas</B> </P>{END Chamadas_Atendidas_fieldheader}</TD>
    <TD class=blackshade>{BEGIN Chamadas_Abandonadas_fieldheader}
      <P align=center><B>Total de chamadas Abandonadas</B> </P>{END Chamadas_Abandonadas_fieldheader}</TD></TR>{END grid_header}{BEGIN grid_row}<!-------------------------------------  data ----------------------------------------->
  {BEGIN row_data}<TR>
    <TD class=data>
      <P align=center>{$Fila_value}</P></TD>
    <TD class=data>
      <P align=center>{$Hora_value}</P></TD>
    <TD class=data>
      <P align=center>{$Agent_Disp_value}</P></TD>
    <TD class=data>
      <P align=center>{$AgentesPausa_value}</P></TD>
    <TD class=data>
      <P align=center>{$Chamadas_Atendidas_value}</P></TD>
    <TD class=data>
      <P align=center>{$Chamadas_Abandonadas_value}</P></TD></TR>{END row_data}{END grid_row}</TBODY></TABLE>{END grid_block}</DIV>{END page}{$footer} {END body}</BODY></HTML>