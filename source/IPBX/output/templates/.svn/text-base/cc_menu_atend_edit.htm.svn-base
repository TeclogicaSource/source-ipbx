<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0 Transitional//EN">
<HTML {$html_attrs}><HEAD><TITLE>Menu de Atendimento</TITLE>
<link REL="stylesheet" href="include/style.css" type="text/css"><!--[if IE]><link REL="stylesheet" href="include/styleIE.css" type="text/css"><![endif]-->
<STYLE>
#center_block{$id} {width:500px;margin:0 auto;}
#contents_block{$id} {text-align:center;width:100%}
#header_block{$id} {width:100%;white-space:nowrap;height:35px;padding-top:5px;text-align:center;border-bottom:1px white solid;}
#fields_block{$id} {width:100%}
#fields_block{$id} td {padding-left:10px}
#buttons_block{$id} {width:100%;padding-bottom:5px;text-align:center}
#required_block{$id} {text-align:left;padding:5px}
#buttons_block{$id} > * {margin:0 2px;text-align:center}
#span{$id} {margin:0 20px 0 20px;}
</STYLE>

<META name=GENERATOR content="MSHTML 8.00.6001.19222"></HEAD>
<BODY>{BEGIN body}{$header} 
{BEGIN editForm}<TABLE style="WIDTH: 80%" id="center_block{$id}" align=center>
  <TBODY>
  <TR>
    <TD id="contents_block{$id}"><B class=xtop><B class=xb1b></B><B 
      class=xb2b></B><B class=xb3b></B><B class=xb4b></B></B>
      <DIV id="header_block{$id}" class=upeditmenu>Menu de Atendimento, 
      Editar registro [ Identificação: {$show_key1} ] </DIV>
      {BEGIN message_block}<DIV id="message_block{$id}" class=downedit>{$message}</DIV>{END message_block}
      <TABLE border=0 cellSpacing=0 cellPadding=4 width="100%">
        <TBODY>
        {BEGIN dsc_menu_fieldblock}<TR>
          <TD style="PADDING-LEFT: 15px" class=editshadeleft_b2 width=150>{BEGIN dsc_menu_label}Descrição Menu{END dsc_menu_label}</TD>
          <TD style="PADDING-LEFT: 10px" class=editshaderight_lb2 
            width=250>{$dsc_menu_editcontrol} </TD></TR>{END dsc_menu_fieldblock}
        {BEGIN nm_tentativas_fieldblock}<TR>
          <TD style="PADDING-LEFT: 15px" class=editshade_b 
          width=150>Repetições</TD>
          <TD style="PADDING-LEFT: 10px" class=editshade_lb width=250>{$nm_tentativas_editcontrol} </TD></TR>{END nm_tentativas_fieldblock}
        {BEGIN ramal_acesso_fieldblock}<TR>
          <TD style="PADDING-LEFT: 15px" class=editshade_b width=150>Ramal 
            Acesso</TD>
          <TD style="PADDING-LEFT: 10px" class=editshade_lb width=250>{$ramal_acesso_editcontrol} </TD></TR>{END ramal_acesso_fieldblock}
        {BEGIN destino_fieldblock}<TR>
          <TD style="PADDING-LEFT: 15px" class=editshade_b 
width=150>Destino</TD>
          <TD style="PADDING-LEFT: 10px" class=editshade_lb width=250>{$destino_editcontrol} </TD></TR>{END destino_fieldblock}
        {BEGIN flg_dig_ramal_fieldblock}<TR>
          <TD style="PADDING-LEFT: 15px" class=editshade_b width=150>Inclui 
            discagem de ramal</TD>
          <TD style="PADDING-LEFT: 10px" class=editshade_lb width=250>{$flg_dig_ramal_editcontrol} </TD></TR>{END flg_dig_ramal_fieldblock}
        {BEGIN arq_audio_fieldblock}<TR>
          <TD style="PADDING-LEFT: 15px" class=editshade_b width=150>Audio do 
            menu (WAV)</TD>
          <TD style="PADDING-LEFT: 10px" class=editshade_lb width=250>{$arq_audio_editcontrol} </TD></TR>{END arq_audio_fieldblock}
        <TR>
          <TD style="BACKGROUND-COLOR: #c0c0c0; WHITE-SPACE: normal" 
            colSpan=2>&nbsp;<STRONG>Configure o menu conforme desejado acima, ou 
            um fluxograma personalizado abaixo.</STRONG></TD></TR>
        {BEGIN id_fluxo_fieldblock}<TR>
          <TD style="PADDING-LEFT: 15px" class=editshade_b 
          width=150>Fluxograma</TD>
          <TD style="PADDING-LEFT: 10px" class=editshade_lb width=250>{$id_fluxo_editcontrol} </TD></TR>{END id_fluxo_fieldblock}</TBODY></TABLE>
      <DIV id="buttons_block{$id}" class=downedit2>
      {BEGIN legend}<DIV id="required_block{$id}"><IMG alt=bullet 
      src="images/icon_required.gif"> - Required field</DIV>{END legend}{$legendBreak} {BEGIN save_button}<SPAN class=buttonborder><INPUT id="savebutton{$id}" class=button value=Salvar type=submit {$savebutton_attrs}></SPAN>{END save_button} 
      {BEGIN reset_button}<SPAN class=buttonborder><INPUT class=button value=Limpar type=reset {$resetbutton_attrs}></SPAN>{END reset_button} 
      <SPAN id="span{$id}">&nbsp;</SPAN> {BEGIN prev_button}<SPAN class=buttonborder><INPUT id="prev{$id}" class=button value=" <<< " type=reset {$prevbutton_attrs}></SPAN>{END prev_button} 
      {BEGIN back_button}<SPAN class=buttonborder><INPUT class=button value="Voltar à Lista" type=button {$backbutton_attrs}></SPAN>{END back_button} 
      {BEGIN next_button}<SPAN class=buttonborder><INPUT id="next{$id}" class=button value=" >>> " type=reset {$nextbutton_attrs}></SPAN>{END next_button} 
      </DIV><B class=xbottom><B class=xb4a></B><B class=xb3a></B><B 
      class=xb2a></B><B class=xb1a></B></B></TD></TR></TBODY></TABLE>{END editForm}{BEGIN detail_tables}{$displayDetailTable_cc_menu_atend_item} 
{END detail_tables}<BR><BR>{$footer} {END body}</BODY></HTML>