<html {$html_attrs}>
<head>
<title>Contatos</title>
<link REL="stylesheet" href="include/style.css" type="text/css">
<!--[if IE]>
<link REL="stylesheet" href="include/styleIE.css" type="text/css">
<![endif]-->
<style>
.grid_block {  
margin-top:5px;
border-collapse: collapse;
align:center;
width:95%;
border:none;
} 
.grid_block td { 
 padding: 5px; 
 margin: 0px; 
 border: solid black 1px;
}
.grid_block .gridspacing {width:20px;border-top-color:white; border-bottom-color:white;background-color:white}
@media print 
{
	a.pdf {display:none}
}
.printpage {page-break-after:always}
#selpage{margin-left:30px;}
</style> 
</head>
<body style="background-color:white">
{BEGIN body}
{$header}
{BEGIN page}

<h1>Contatos</h1>
{BEGIN page_number}
<div id='selpage'>Página {$pageno} de {$maxpages} </div>
{END page_number}

{BEGIN grid_block}

<table align="center" cellpadding=5 class="grid_block">
{BEGIN grid_header}
<tr>
{BEGIN record_header}
{BEGIN Nome_fieldheadercolumn}
	<td>
{BEGIN Nome_fieldheader}
	<b>Nome</b>
{END Nome_fieldheader}
	</td>
{END Nome_fieldheadercolumn}
{BEGIN Numero_fieldheadercolumn}
	<td>
{BEGIN Numero_fieldheader}
	<b>Número</b>
{END Numero_fieldheader}
	</td>
{END Numero_fieldheadercolumn}
{BEGIN Discar_fieldheadercolumn}
	<td>
{BEGIN Discar_fieldheader}
	<b>Discar</b>
{END Discar_fieldheader}
	</td>
{END Discar_fieldheadercolumn}
{BEGIN idt_custo_fieldheadercolumn}
	<td>
{BEGIN idt_custo_fieldheader}
	<b>Centro de custo</b>
{END idt_custo_fieldheader}
	</td>
{END idt_custo_fieldheadercolumn}
{BEGIN idt_agenda_fieldheadercolumn}
	<td>
{BEGIN idt_agenda_fieldheader}
	<b>Atalho Agenda</b>
{END idt_agenda_fieldheader}
	</td>
{END idt_agenda_fieldheadercolumn}
{BEGIN endrecordheader_block}
	<td class="gridspacing">&nbsp;</td>
{END endrecordheader_block}
{END record_header}
</tr>
{END grid_header}
{BEGIN grid_row}
<tr valign=top {$rowstyle} {$rowattrs}>
{BEGIN grid_record}
{BEGIN Nome_fieldcolumn}
<td {$Nome_style}>
{$Nome_value}
</td>
{END Nome_fieldcolumn}
{BEGIN Numero_fieldcolumn}
<td {$Numero_style}>
{$Numero_value}
</td>
{END Numero_fieldcolumn}
{BEGIN Discar_fieldcolumn}
<td {$Discar_style}>
{$Discar_value}
</td>
{END Discar_fieldcolumn}
{BEGIN idt_custo_fieldcolumn}
<td {$idt_custo_style}>
{$idt_custo_value}
</td>
{END idt_custo_fieldcolumn}
{BEGIN idt_agenda_fieldcolumn}
<td {$idt_agenda_style}>
{$idt_agenda_value}
</td>
{END idt_agenda_fieldcolumn}
{BEGIN endrecord_block}
<td class="gridspacing">&nbsp;</td>
{END endrecord_block}
{END grid_record}
</tr>
{END grid_row}
<!-- totals row -->
</table>
{END grid_block}
{END page}
{$footer}
{END body}
</body>
</html>