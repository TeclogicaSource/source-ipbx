<!doctype html>
<html  {$html_attrs}>
	<head>
		<meta http-equiv="X-UA-Compatible" content="IE=Edge">
		<title>{$pagetitle}</title>

		<meta http-equiv="Content-Type" content="text/html; charset=Windows-1252">
		<link REL="stylesheet" href="styles/default.css" type="text/css">
		{BEGIN rtlCSS}
		<link REL="stylesheet" href="styles/defaultRTL.css" type="text/css">
		{END rtlCSS}
		{BEGIN styleCSSFiles}
		<link REL="stylesheet" href="{$stylepath}" type="text/css">
		{END styleCSSFiles}
		{BEGIN pageCSSFiles}
		<link REL="stylesheet" href="{$pagestylepath}" type="text/css">
		{END pageCSSFiles}
	</head>
	
	<body class="{$stylename} function-rprint" {$bodyattrs}>
	{BEGIN body}
		{$header}
	<div class="rnr-page">
		<div class="rnr-top {$blockclass_top}">
			

{BEGIN container_pdf}
<div class="rnr-c rnr-ch rnr-c-pdf">

{BEGIN printbuttons}
<div  class="style1 rnr-bl rnr-b-printbuttons {$brickclass_printbuttons}">{BEGIN divideintopages_block}
<a href="#" class="divideintopages rnr-button button" {$divideintopages_attrs}>Prepare for printing</a>
{END divideintopages_block}
</div>
{END printbuttons}

<div class="rnr-hfiller"></div>
</div>
{END container_pdf}





{BEGIN container_pageheader}
<div class="rnr-c rnr-cv rnr-c-pageheader">


<style>
	td.pagesummary.pagesummary.pagesummary.pagesummary.pagesummary.pagesummary.pagesummary.pagesummary {background-color:#ffffbb;}
	td.globalsummary.globalsummary.globalsummary.globalsummary.globalsummary.globalsummary.globalsummary.globalsummary {background-color:#ffe0b0;}
	.rnr-report {width: 100%;}
	td {padding: 2px; 4px;}

		td.group_1{background-color: #E6E6FA;}
		td.nbgroup_1{background-color: #E6E6FA;}
		td.header_1 {
			background-color: #C3C3D4; 
			font-size:120%;
			padding: 2px 8px;
			font-weight: bold;
		}

		td.group_2{background-color: #C0C0C0;}
		td.nbgroup_2{background-color: #C0C0C0;}
		td.header_2 {
			background-color: #A3A3A3; 
			font-size:120%;
			padding: 2px 8px;
			font-weight: bold;
		}
	td.blackshade {
		font-size:140%;
		padding: 4px 8px;
	}
	.summaryrow > * {
		border:solid #808080 1px; 
		border-left: none; 
		border-right: none;
	}
	.grouptotalsrow + :not(.grouptotalsrow) > * {
		border-top:solid #808080 1px; 
	}

</style>



{BEGIN printheader}
<div  class="rnr-brickcontents style1 rnr-b-printheader {$brickclass_printheader}"><span>	<h1>{$pagetitlelabel Rel__Simplificado___Centro_de_custo rprint}</h1>
</span></div>{END printheader}


{BEGIN page_number}
<div  class="rnr-brickcontents style1 rnr-b-page_of_print {$brickclass_page_of_print}"><h1>Página <span class="rp-pageno">{$pageno}</span> de <span class="rp-pagecount">{$pagecount}</span></h1></div>{END page_number}


</div>
{END container_pageheader}


		</div>
		<div class="rnr-middle">
			<div class="rnr-left {$blockclass_left}">
				
			</div>
			<div class="rnr-center {$blockclass_center}">
				

{BEGIN container_grid}



<style>
	td.pagesummary.pagesummary.pagesummary.pagesummary.pagesummary.pagesummary.pagesummary.pagesummary {background-color:#ffffbb;}
	td.globalsummary.globalsummary.globalsummary.globalsummary.globalsummary.globalsummary.globalsummary.globalsummary {background-color:#ffe0b0;}
	.rnr-report {width: 100%;}
	td {padding: 2px; 4px;}

		td.group_1{background-color: #E6E6FA;}
		td.nbgroup_1{background-color: #E6E6FA;}
		td.header_1 {
			background-color: #C3C3D4; 
			font-size:120%;
			padding: 2px 8px;
			font-weight: bold;
		}

		td.group_2{background-color: #C0C0C0;}
		td.nbgroup_2{background-color: #C0C0C0;}
		td.header_2 {
			background-color: #A3A3A3; 
			font-size:120%;
			padding: 2px 8px;
			font-weight: bold;
		}
	td.blackshade {
		font-size:140%;
		padding: 4px 8px;
	}
	.summaryrow > * {
		border:solid #808080 1px; 
		border-left: none; 
		border-right: none;
	}
	.grouptotalsrow + :not(.grouptotalsrow) > * {
		border-top:solid #808080 1px; 
	}

</style>

<div id="grid_block{$id}" class="rnr-report-gridblock">
		<div class="rnr-cw-grid rnr-s-empty asbuttons {$pageStyleName}">		
						<div class="rnr-c rnr-cv rnr-c-grid">
		<div class="rnr-brickcontents style1 rnr-b-report">		
			<table class="rnr-report" cellspacing=0 cellpadding=0 border=0>
<tbody>
			
			
			{BEGIN grid_row}

			{BEGIN Ano_newgroup}
								<tr class="group-headers">
					<td class="blackshade">{$label Rel__Simplificado___Centro_de_custo Ano}</td>
					<td class="blackshade">{$label Rel__Simplificado___Centro_de_custo Mes}</td>
										<td class="blackshade">{$label Rel__Simplificado___Centro_de_custo dsc_interf}</td>
					<td class="blackshade">{$label Rel__Simplificado___Centro_de_custo dsc_centro_cust}</td>
					<td class="blackshade">{$label Rel__Simplificado___Centro_de_custo custo}</td>
				</tr>
				<tr class="group-spliter-1" >
																				<td colspan=5 class=header_1><font >{$Ano_grval}</font></td>
										</tr>
			{END Ano_newgroup}
			{BEGIN Mes_newgroup}
								<tr class="group-spliter-2" {$rowattrs}>
													<td class=" group_1" >&nbsp;</td>
													
							<td colspan=4 class=header_2>
								<font>
									{$Mes_grval} 

								</font>
							</td>
				</tr>
			{END Mes_newgroup}
						<!-------------------------------------  data ----------------------------------------->
			{BEGIN row_data}
				<tr >
										<td class=" group_1" >&nbsp;</td>
										<td class=" group_2" >&nbsp;</td>
									<td class=data>{$dsc_interf_value}</td>
					<td class=data>{$dsc_centro_cust_value}</td>
					<td class=data>{$custo_value}</td>
				</tr>
			{END row_data}
			<!-------------------------------------  group summary ----------------------------------------->
						{BEGIN Mes_endgroup}
				<tr class="summaryrow" >
					<td class=group_1 style="border-top: none;"></td>
										<td  class="group_2" colspan=4 >
				Resumo para {$label Rel__Simplificado___Centro_de_custo Mes} {$Mes_grval}
					- <span class="summary-2" data-type="summary" data-value="{$groupMes_total_cnt}">{$groupMes_total_cnt}</span> Total de registros</td>
				</tr>
						<!--  Sum -->
				<tr class="grouptotalsrow" >
										<td class=group_1 >&nbsp;</td>
										<td class=group_2 >Soma</td>
										<td class=group_2>&nbsp;</td>
										<td class=group_2>&nbsp;</td>
										<td class=group_2><span class="summary-2" data-type="sum" data-value="{$groupMes_totalcusto_sum}" data-field="custo">{$groupMes_totalcusto_sum}</span></td>
				</tr>	
												{END Mes_endgroup}
						{BEGIN Ano_endgroup}
				<tr class="summaryrow" >
										<td  class="group_1" colspan=5 >
				Resumo para {$label Rel__Simplificado___Centro_de_custo Ano} {$Ano_grval}
					- <span class="summary-1" data-type="summary" data-value="{$groupAno_total_cnt}">{$groupAno_total_cnt}</span> Total de registros</td>
				</tr>
						<!--  Sum -->
				<tr class="grouptotalsrow" >
										<td class=group_1 >Soma</td>
										<td class=group_1>&nbsp;</td>
										<td class=group_1>&nbsp;</td>
										<td class=group_1>&nbsp;</td>
										<td class=group_1><span class="summary-1" data-type="sum" data-value="{$groupAno_totalcusto_sum}" data-field="custo">{$groupAno_totalcusto_sum}</span></td>
				</tr>	
												{END Ano_endgroup}
			{END grid_row}

							</tbody>
			</table>
		</div>
	</div>

	</div>
</div>





{END container_grid}


			</div>
			<div class="rnr-right {$blockclass_right}">
				
			</div>
		</div>
		<div class="rnr-bottom {$blockclass_bottom}">
			<!--%%bottom%%-->
		</div>
	</div>
		
		{$footer}
	{END body}
	</body>
</html>
