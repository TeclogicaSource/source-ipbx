<html {$html_attrs}>
<head>
<title>Custos</title>
{BEGIN stylesheetlink}
<link REL="stylesheet" href="include/style.css" type="text/css">
<!--[if IE]>
<link REL="stylesheet" href="include/styleIE.css" type="text/css">
<![endif]-->
{END stylesheetlink}
<style>
.grid_block {  
margin-top:5px;
border-collapse: collapse;
align:center;
width:95%;
border: solid black 1px;
} 
.grid_block td { 
 padding: 5px; 
 margin: 0px; 
 border: solid #cccccc 1px;
}
.grid_block .gridspacing {width:20px;border-top-color:white; border-bottom-color:white;background-color:white}
@media print 
{
	a.pdf {display:none}
}
.printpage {page-break-after:always}
.pagesummary
{
background-color:#ffffbb;
}
.globalsummary
{
background-color:#ffe0b0;
}

{$cssdata}


.group_1
{
background-color: #FFFFFF;
}


.nbgroup_1
{
background-color: #FFFFFF;
border-style:none;
}


.header_1
{
background-color: #D8D8D8;
}


</style> 
</head>
<body topmargin=5 style="background:white">
{BEGIN body}
{$header}
{BEGIN page}
<div>

<h1>Custos</h1>
<div id='selpage'>Página {$pageno} de {$maxpages} </div>

{BEGIN grid_block}
<table align="center" cellpadding=5 class="grid_block">
{BEGIN grid_header}
<tr>
<td class=blackshade>
{BEGIN clid_fieldheader}
<b>Usuário</b>
{END clid_fieldheader}
</td>
<td class=blackshade>
{BEGIN calldate_fieldheader}
<b>Calldate</b>
{END calldate_fieldheader}
</td>
<td class=blackshade>
{BEGIN dst_fieldheader}
<b>Destino</b>
{END dst_fieldheader}
</td>
</tr>
{END grid_header}
{BEGIN grid_row}
<!-------------------------------------  group headers ----------------------------------------->
{BEGIN clid_firstnewgroup}
	<tr>
				<td class=header_1>
		<font style="font-weight:900">{$clid_grval}</font>
				</td>
<!-------------------------------------  data ----------------------------------------->
	<td class=data>
		&nbsp;
	<td class=data>
		&nbsp;
	</tr>
{END clid_firstnewgroup}
<!-------------------------------------  group summary ----------------------------------------->
{BEGIN clid_endgroup}
<tr>
	<td class=group_1
		style="border-bottom: solid black 2px"
	colspan=3 >
	Resumo para Usuário {$clid_grval}
	 - {$groupclid_total_cnt} Total de registros</div></td>
</tr>
{END clid_endgroup}
{END grid_row}
</table>
{END grid_block}
</div>
{END page}
{$footer}
{END body}
</body>
</html>
