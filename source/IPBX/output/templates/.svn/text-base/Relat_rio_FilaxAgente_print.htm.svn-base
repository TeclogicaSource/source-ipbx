<html {$html_attrs}>
<head>
<title>Fila X Agente</title>
{BEGIN stylesheetlink}
<link REL="stylesheet" href="include/style.css" type="text/css">
<!--[if IE]>
<link REL="stylesheet" href="include/styleIE.css" type="text/css">
<![endif]-->
{END stylesheetlink}
<style>
.grid_block {  
margin-top:5px;
border-collapse: collapse;
align:center;
width:95%;
border: solid black 1px;
} 
.grid_block td { 
 padding: 5px; 
 margin: 0px; 
 border: solid #cccccc 1px;
}
.grid_block .gridspacing {width:20px;border-top-color:white; border-bottom-color:white;background-color:white}
@media print 
{
	a.pdf {display:none}
}
.printpage {page-break-after:always}
.pagesummary
{
background-color:#ffffbb;
}
.globalsummary
{
background-color:#ffe0b0;
}

{$cssdata}

.group_1
{
background-color: #C0C0C0;
}


.nbgroup_1
{
background-color: #C0C0C0;
border-style:none;
}


.header_1
{
background-color: #A3A3A3;
}

</style> 
</head>
<body topmargin=5 style="background:white">
{BEGIN body}
{$header}
{BEGIN page}
<div>

<h1>Fila X Agente</h1>
<div id='selpage'>Página {$pageno} de {$maxpages} </div>

{BEGIN grid_block}
<table align="center" cellpadding=5 class="grid_block">
{BEGIN grid_row}
<!-------------------------------------  group headers ----------------------------------------->
{BEGIN Fila_newgroup}
<tr>
<td class=header_1 colspan=10>
Fila:&nbsp;
<font style="font-weight:900">{$Fila_grval}</font></td>
</tr>
<tr>
<td class=blackshade><b>Data de Entrada</b></td>
<td class=blackshade><b>Hora de Entrada</b></td>
<td class=blackshade><b>Telefone</b></td>
<td class=blackshade><b>Hora Atendimento</b></td>
<td class=blackshade><b>Evento</b></td>
<td class=blackshade><b>Agente</b></td>
<td class=blackshade><b>Tempo de Espera</b></td>
<td class=blackshade><b>Tempo de Atendimento</b></td>
<td class=blackshade><b>Hora de Abandono</b></td>
<td class=blackshade><b>Telefone Transferido</b></td>
</tr>
{END Fila_newgroup}
<!-------------------------------------  data ----------------------------------------->
{BEGIN row_data}
<tr>
	<td class=data>{$Data_de_Entrada_value}</td>
	<td class=data>{$Hora_de_Entrada_value}</td>
	<td class=data>{$Telefone_value}</td>
	<td class=data>{$Hora_Atendimento_value}</td>
	<td class=data>{$Evento_value}</td>
	<td class=data>{$Agente_value}</td>
	<td class=data>{$Tempo_de_Espera_value}</td>
	<td class=data>{$Tempo_de_Atendimento_value}</td>
	<td class=data>{$Hora_de_Abandono_value}</td>
	<td class=data>{$Telefone_Transferido_value}</td>
</tr>
{END row_data}
<!-------------------------------------  group summary ----------------------------------------->
{END grid_row}
</table>
{END grid_block}
</div>
{END page}
{$footer}
{END body}
</body>
</html>