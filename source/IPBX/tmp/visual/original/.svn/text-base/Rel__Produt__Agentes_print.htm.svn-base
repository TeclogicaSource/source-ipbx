<html {$html_attrs}>
<head>
<title>Rel. Produt. Agentes</title>
{BEGIN stylesheetlink}
<link REL="stylesheet" href="include/style.css" type="text/css">
<!--[if IE]>
<link REL="stylesheet" href="include/styleIE.css" type="text/css">
<![endif]-->
{END stylesheetlink}
<style>
.grid_block {  
margin-top:5px;
border-collapse: collapse;
align:center;
width:95%;
border: solid black 1px;
} 
.grid_block td { 
 padding: 5px; 
 margin: 0px; 
 border: solid #cccccc 1px;
}
.grid_block .gridspacing {width:20px;border-top-color:white; border-bottom-color:white;background-color:white}
@media print 
{
	a.pdf {display:none}
}
.printpage {page-break-after:always}
.pagesummary
{
background-color:#ffffbb;
}
.globalsummary
{
background-color:#ffe0b0;
}

{$cssdata}


.group_1
{
background-color: #7F7F7F;
}
.group_2
{
background-color: #C0C0C0;
}


.nbgroup_1
{
background-color: #7F7F7F;
border-style:none;
}
.nbgroup_2
{
background-color: #C0C0C0;
border-style:none;
}


.header_1
{
background-color: #6B6B6B;
}
.header_2
{
background-color: #A3A3A3;
}


</style> 
</head>
<body topmargin=5 style="background:white">
{BEGIN body}
{$header}
{BEGIN page}
<div>
<h1>Rel. Produt. Agentes</h1>

<div id='selpage'>Página {$pageno} de {$maxpages} </div>


{BEGIN grid_block}
<table align="center" cellpadding=5 class="grid_block">
{BEGIN grid_header}
<tr>
<td class=blackshade>
{BEGIN Ano_fieldheader}
<b>Ano</b>
{END Ano_fieldheader}
</td>
<td class=blackshade>
{BEGIN Mes_fieldheader}
<b>Mes</b>
{END Mes_fieldheader}
</td>
<td class=blackshade>
{BEGIN fila_fieldheader}
<b>Fila</b>
{END fila_fieldheader}
</td>
<td class=blackshade>
{BEGIN Agente_fieldheader}
<b>Agente</b>
{END Agente_fieldheader}
</td>
<td class=blackshade>
{BEGIN tot_logado_fieldheader}
<b>Total Disponível</b>
{END tot_logado_fieldheader}
</td>
<td class=blackshade>
{BEGIN qtd_atend_fieldheader}
<b>Atendidas</b>
{END qtd_atend_fieldheader}
</td>
<td class=blackshade>
{BEGIN tot_atend_fieldheader}
<b>Total Atendimento</b>
{END tot_atend_fieldheader}
</td>
<td class=blackshade>
{BEGIN med_atend_fieldheader}
<b>Média Atendimento</b>
{END med_atend_fieldheader}
</td>
<td class=blackshade>
{BEGIN tot_pausa_n_produtiva_fieldheader}
<b>Total Pausa não Produt.</b>
{END tot_pausa_n_produtiva_fieldheader}
</td>
<td class=blackshade>
{BEGIN tot_pausa_produtiva_fieldheader}
<b>Total Pausa Produt.</b>
{END tot_pausa_produtiva_fieldheader}
</td>
<td class=blackshade>
{BEGIN tot_pausa_fieldheader}
<b>Total Pausa</b>
{END tot_pausa_fieldheader}
</td>
<td class=blackshade>
{BEGIN tot_parado_fieldheader}
<b>Total Livre</b>
{END tot_parado_fieldheader}
</td>
<td class=blackshade>
{BEGIN qtd_nao_atend_fieldheader}
<b>Rejeitadas/N Atendidas</b>
{END qtd_nao_atend_fieldheader}
</td>
<td class=blackshade>
{BEGIN tot_recebida_fieldheader}
<b>Recebidas</b>
{END tot_recebida_fieldheader}
</td>
<td class=blackshade>
{BEGIN cntrb_indiv_fieldheader}
<b>Contribuição Individual</b>
{END cntrb_indiv_fieldheader}
</td>
</tr>
{END grid_header}
{BEGIN grid_row}
<!-------------------------------------  group headers ----------------------------------------->
{BEGIN Ano_newgroup}
<tr>
		<td class=header_1 colspan=15><font style="font-weight:900">{$Ano_grval}</font></td>
		
</tr>
{END Ano_newgroup}
{BEGIN Mes_newgroup}
<tr>
		<td class="noborder group_1" style="border-style:none;">&nbsp;</td>

		<td class=header_2 colspan=14><font style="font-weight:900">{$Mes_grval}</font></td>

</tr>
{END Mes_newgroup}
<!-------------------------------------  data ----------------------------------------->
{BEGIN row_data}
<tr>
			<td style="BORDER-BOTTOM-STYLE: none; BORDER-TOP-STYLE: none; BORDER-LEFT-STYLE: none; BORDER-RIGHT: 1px solid" class="noborder group_1">&nbsp;</td>
			<td style="BORDER-BOTTOM-STYLE: none; BORDER-TOP-STYLE: none; BORDER-LEFT-STYLE: none; BORDER-RIGHT: 1px solid" class="noborder group_2">&nbsp;</td>
	<td class=data>{$fila_value}</td>
	<td class=data>{$Agente_value}</td>
	<td class=data>{$tot_logado_value}</td>
	<td class=data>{$qtd_atend_value}</td>
	<td class=data>{$tot_atend_value}</td>
	<td class=data>{$med_atend_value}</td>
	<td class=data>{$tot_pausa_n_produtiva_value}</td>
	<td class=data>{$tot_pausa_produtiva_value}</td>
	<td class=data>{$tot_pausa_value}</td>
	<td class=data>{$tot_parado_value}</td>
	<td class=data>{$qtd_nao_atend_value}</td>
	<td class=data>{$tot_recebida_value}</td>
	<td class=data>{$cntrb_indiv_value}</td>
</tr>
{END row_data}
<!-------------------------------------  group summary ----------------------------------------->
{END grid_row}
</table>
{END grid_block}
</div>
{END page}
{$footer}
{END body}
</body>
</html>